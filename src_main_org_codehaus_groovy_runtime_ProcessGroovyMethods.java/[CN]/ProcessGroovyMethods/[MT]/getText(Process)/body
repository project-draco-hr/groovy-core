{
  String text=IOGroovyMethods.getText(new BufferedReader(new InputStreamReader(self.getInputStream())));
  try {
    self.getErrorStream().close();
  }
 catch (  IOException ignore) {
  }
  try {
    self.getInputStream().close();
  }
 catch (  IOException ignore) {
  }
  try {
    self.getOutputStream().close();
  }
 catch (  IOException ignore) {
  }
  return text;
}
