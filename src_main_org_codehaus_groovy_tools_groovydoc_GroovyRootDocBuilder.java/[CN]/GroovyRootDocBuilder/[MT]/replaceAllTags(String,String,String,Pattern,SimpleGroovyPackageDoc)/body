{
  Matcher matcher=regex.matcher(self);
  if (matcher.find()) {
    matcher.reset();
    StringBuffer sb=new StringBuffer();
    while (matcher.find()) {
      String tagname=matcher.group(1);
      if (tagname.equals("see") || tagname.equals("link")) {
        matcher.appendReplacement(sb,s1 + SimpleGroovyClassDoc.getDocUrl(SimpleGroovyClassDoc.encodeSpecialSymbols(matcher.group(2)),false,links,packageDoc.getRelativeRootPath(),rootDoc,null) + s2);
      }
 else       if (!tagname.equals("interface")) {
        matcher.appendReplacement(sb,s1 + SimpleGroovyClassDoc.encodeSpecialSymbols(matcher.group(2)) + s2);
      }
    }
    matcher.appendTail(sb);
    return sb.toString();
  }
 else {
    return self;
  }
}
