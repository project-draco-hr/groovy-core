{
  if (object != null) {
    if (object instanceof List) {
      List list=(List)object;
      List answer=new ArrayList();
      Iterator it=list.iterator();
      for (; it.hasNext(); ) {
        answer.add(getAttributeSafe(it.next(),attribute));
      }
      return answer;
    }
 else     return getAttributeSafe(object,attribute);
  }
  return null;
}
