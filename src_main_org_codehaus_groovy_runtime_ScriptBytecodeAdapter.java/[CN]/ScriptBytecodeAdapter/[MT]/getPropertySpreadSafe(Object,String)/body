{
  if (object != null) {
    if (object instanceof List) {
      List list=(List)object;
      List answer=new ArrayList();
      Iterator it=list.iterator();
      for (; it.hasNext(); ) {
        answer.add(getPropertySafe(it.next(),property));
      }
      return answer;
    }
 else     return getPropertySafe(object,property);
  }
  return null;
}
