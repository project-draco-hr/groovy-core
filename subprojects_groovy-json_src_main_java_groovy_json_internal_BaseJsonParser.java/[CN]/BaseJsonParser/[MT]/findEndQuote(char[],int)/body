{
  char currentChar;
  boolean escape=false;
  for (; index < array.length; index++) {
    currentChar=array[index];
    if (isDoubleQuote(currentChar)) {
      if (!escape) {
        break;
      }
    }
    if (isEscape(currentChar)) {
      escape=true;
    }
 else {
      escape=false;
    }
  }
  return index;
}
