{
  int size=getParameterCount();
  if (sender != that.sender)   return false;
  if (name.equals(that.name) && size == that.getParameterCount()) {
    for (int i=0; i < size; i++) {
      if (!getParameterType(i).equals(that.getParameterType(i))) {
        return false;
      }
    }
    return true;
  }
  return false;
}
