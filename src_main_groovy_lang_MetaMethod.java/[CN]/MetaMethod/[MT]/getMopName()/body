{
  if (mopName == null) {
    String name=getName();
    CachedClass declaringClass=getDeclaringClass();
    if ((getModifiers() & (Modifier.PUBLIC | Modifier.PROTECTED)) == 0)     mopName=new StringBuffer().append("this$").append(declaringClass.getSuperClassDistance()).append("$").append(name).toString();
 else     mopName=new StringBuffer().append("super$").append(declaringClass.getSuperClassDistance()).append("$").append(name).toString();
  }
  return mopName;
}
