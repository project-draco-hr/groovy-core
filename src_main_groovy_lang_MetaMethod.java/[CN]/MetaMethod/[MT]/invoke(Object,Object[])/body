{
  if (reflector != null) {
    if (!MetaClass.isValidMethod(getParameterTypes(),arguments,false)) {
      throw new IllegalArgumentException("Parameters to method: " + getName() + " do not match types: "+ InvokerHelper.toString(getParameterTypes())+ " for arguments: "+ InvokerHelper.toString(arguments));
    }
    return reflector.invoke(this,object,arguments);
  }
 else {
    log.warning("No reflector available for method so must use reflection: " + method);
    method.setAccessible(true);
    return method.invoke(object,arguments);
  }
}
