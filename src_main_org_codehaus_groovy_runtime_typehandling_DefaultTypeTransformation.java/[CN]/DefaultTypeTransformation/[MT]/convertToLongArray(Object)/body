{
  long[] ans=null;
  if (a.getClass().getName().equals("[J")) {
    ans=(long[])a;
  }
 else {
    Object[] ia=(Object[])a;
    ans=new long[ia.length];
    for (int i=0; i < ia.length; i++) {
      if (ia[i] == null) {
        continue;
      }
      ans[i]=((Number)ia[i]).longValue();
    }
  }
  return ans;
}
