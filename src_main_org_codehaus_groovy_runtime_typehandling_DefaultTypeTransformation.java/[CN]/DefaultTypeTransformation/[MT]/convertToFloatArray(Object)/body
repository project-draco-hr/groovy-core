{
  float[] ans=null;
  if (a.getClass().getName().equals("[F")) {
    ans=(float[])a;
  }
 else {
    Object[] ia=(Object[])a;
    ans=new float[ia.length];
    for (int i=0; i < ia.length; i++) {
      if (ia[i] == null) {
        continue;
      }
      ans[i]=((Number)ia[i]).floatValue();
    }
  }
  return ans;
}
