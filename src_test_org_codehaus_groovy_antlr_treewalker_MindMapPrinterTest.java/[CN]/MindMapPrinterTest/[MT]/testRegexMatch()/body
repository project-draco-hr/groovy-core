{
  assertXmlEquals(HEADER + "<node TEXT='if  &lt;134&gt;' POSITION='right' COLOR=\"#17178B\">" + "  <node TEXT='an identifier &lt;84&gt; : foo' POSITION='right' COLOR=\"#006699\"></node>"+ "  <node TEXT='==~  &lt;172&gt;' POSITION='right' COLOR=\"#000000\"></node>"+ "  <node TEXT='a string literal &lt;130&gt; : bar' POSITION='right' COLOR=\"#008000\"></node>"+ "</node>"+ "<node TEXT='SLIST &lt;7&gt; : {' POSITION='right' COLOR=\"#006699\"></node>"+ FOOTER,pretty("if (foo==~\"bar\"){}"));
}
