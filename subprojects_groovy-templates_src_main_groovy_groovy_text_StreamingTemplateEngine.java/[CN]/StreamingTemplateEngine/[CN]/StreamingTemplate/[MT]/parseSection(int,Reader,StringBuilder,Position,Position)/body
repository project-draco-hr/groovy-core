{
  append(target,targetPosition,"          ");
  append(target,targetPosition,(char)pendingC);
  while (true) {
    int c=read(reader,sourcePosition);
    if (c == '%') {
      c=read(reader,sourcePosition);
      if (c == '>')       break;
      append(target,targetPosition,'%');
    }
    append(target,targetPosition,(char)c);
  }
  append(target,targetPosition,';');
}
