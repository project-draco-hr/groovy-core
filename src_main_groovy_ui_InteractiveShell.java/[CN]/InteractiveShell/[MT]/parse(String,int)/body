{
  boolean parsed=false;
  parser=null;
  error=null;
  try {
    parser=SourceUnit.create("groovysh script",code,tolerance);
    parser.parse();
    parsed=true;
  }
 catch (  CompilationFailedException e) {
    if (parser.getErrorCollector().getErrorCount() > 1 || !parser.failedWithUnexpectedEOF()) {
      error=e;
    }
  }
catch (  Exception e) {
    error=e;
  }
  return parsed;
}
