{
  try {
    return method.invoke(object,arguments);
  }
 catch (  IllegalAccessError e) {
    throw new InvokerInvocationException(e);
  }
catch (  IllegalArgumentException e) {
    throw new InvokerInvocationException(e);
  }
catch (  IllegalAccessException e) {
    throw new InvokerInvocationException(e);
  }
catch (  InvocationTargetException e) {
    throw new InvokerInvocationException(e);
  }
}
