{
  boolean[] ans=null;
  if (a.getClass().getName().equals("[Z")) {
    ans=(boolean[])a;
  }
 else {
    Object[] ia=(Object[])a;
    ans=new boolean[ia.length];
    for (int i=0; i < ia.length; i++) {
      if (ia[i] == null) {
        continue;
      }
      ans[i]=((Boolean)ia[i]).booleanValue();
    }
  }
  return ans;
}
