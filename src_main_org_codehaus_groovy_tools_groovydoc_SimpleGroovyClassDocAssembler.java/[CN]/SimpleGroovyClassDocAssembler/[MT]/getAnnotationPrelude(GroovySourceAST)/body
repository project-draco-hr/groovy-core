{
  LineColumn thisLineCol=new LineColumn(t.getLine(),t.getColumn());
  String result="";
  String text=sourceBuffer.getSnippet(lastLineCol,thisLineCol);
  if (text != null) {
    Matcher m=ANNOTATION_PRELUDE_PATTERN.matcher(text);
    if (m.find()) {
      result=m.group(1);
    }
  }
  return result;
}
