{
  assertXmlEquals(HEADER + "<code title='\"if\"'><font color='#17178B'>" + "  <code title='an identifier'><font color='#000000'></font></code>"+ "  <code title=\"'==~'\"><font color='#000000'></font></code>"+ "  <code title='a string literal'><font color='#008000'></font></code>"+ "</font></code>"+ "<code title='SLIST'><font color='#000000'></font></code>"+ FOOTER,pretty("if (foo==~\"bar\"){}"));
}
