{
  try {
    parseClass(new InputStreamCharStream(in),file);
  }
 catch (  SyntaxException e) {
    try {
      in.close();
    }
 catch (    Exception hide) {
    }
    throw e;
  }
catch (  RuntimeParserException e) {
    e.throwParserException();
  }
catch (  IOException e) {
    try {
      in.close();
    }
 catch (    Exception hide) {
    }
    throw e;
  }
  in.close();
}
