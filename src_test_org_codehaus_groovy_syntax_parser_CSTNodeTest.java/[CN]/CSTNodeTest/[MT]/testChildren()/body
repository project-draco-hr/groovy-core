{
  CSTNode node=new CSTNode();
  CSTNode childOne=new CSTNode();
  CSTNode childTwo=new CSTNode();
  node.addChild(childOne);
  node.addChild(childTwo);
  assertLength(2,node.getChildren());
  assertSame(childOne,node.getChild(0));
  assertSame(childTwo,node.getChild(1));
  assertSame(childOne,node.getChildren()[0]);
  assertSame(childTwo,node.getChildren()[1]);
}
