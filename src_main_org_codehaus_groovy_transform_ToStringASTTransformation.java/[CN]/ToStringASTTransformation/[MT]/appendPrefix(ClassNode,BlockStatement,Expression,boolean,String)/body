{
  if (first) {
    first=false;
  }
 else {
    body.addStatement(append(result,new ConstantExpression(", ")));
  }
  body.addStatement(new IfStatement(new BooleanExpression(new FieldExpression(cNode.getField("$print$names"))),toStringPropertyName(result,name),new EmptyStatement()));
  return first;
}
