{
  for (  MethodNode method : getDeclaredMethods(setterName)) {
    if (setterName.equals(method.getName()) && ClassHelper.VOID_TYPE == method.getReturnType() && method.getParameters().length == 1) {
      return method;
    }
  }
  ClassNode parent=getSuperClass();
  if (parent != null)   return parent.getSetterMethod(setterName);
  return null;
}
