{
  List result=new ArrayList(3);
  Map declaredMethods=getDeclaredMethodsMap();
  for (Iterator it=declaredMethods.values().iterator(); it.hasNext(); ) {
    MethodNode method=(MethodNode)it.next();
    if (method.isAbstract()) {
      result.add(method);
    }
  }
  if (result.isEmpty()) {
    return null;
  }
 else {
    return result;
  }
}
