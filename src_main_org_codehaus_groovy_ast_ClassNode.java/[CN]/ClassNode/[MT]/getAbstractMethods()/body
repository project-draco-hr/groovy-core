{
  List result=new ArrayList();
  for (Iterator methIt=getAllDeclaredMethods().iterator(); methIt.hasNext(); ) {
    MethodNode method=(MethodNode)methIt.next();
    if (method.isAbstract()) {
      result.add(method);
    }
  }
  if (result.size() == 0) {
    return null;
  }
 else {
    return result;
  }
}
