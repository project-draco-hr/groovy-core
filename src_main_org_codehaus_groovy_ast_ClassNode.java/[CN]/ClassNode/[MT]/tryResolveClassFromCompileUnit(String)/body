{
  CompileUnit compileUnit=getCompileUnit();
  if (compileUnit != null) {
    if (compileUnit.getClass(type) != null) {
      return type;
    }
    try {
      compileUnit.loadClass(type);
      return type;
    }
 catch (    AccessControlException ace) {
      throw ace;
    }
catch (    Throwable e) {
    }
  }
  return null;
}
