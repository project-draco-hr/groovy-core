{
  if (type != null) {
    if (type.equals("int") || type.equals("short") || type.equals("byte")|| type.equals("char")|| type.equals("float")|| type.equals("long")|| type.equals("double")|| type.equals("java.lang.Short")|| type.equals("java.lang.Byte")|| type.equals("java.lang.Character")|| type.equals("java.lang.Integer")|| type.equals("java.lang.Float")|| type.equals("java.lang.Long")|| type.equals("java.lang.Double")|| type.equals("java.math.BigInteger")|| type.equals("java.math.BigDecimal")) {
      return true;
    }
  }
  return false;
}
