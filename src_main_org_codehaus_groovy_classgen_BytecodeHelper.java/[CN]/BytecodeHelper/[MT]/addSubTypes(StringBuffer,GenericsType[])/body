{
  if (types == null)   return;
  ret.append('<');
  for (int i=0; i < types.length; i++) {
    String name=types[i].getName();
    if (types[i].isPlaceholder()) {
      ret.append('T');
      ret.append(name);
      ret.append(';');
    }
 else {
      writeGenericsBounds(ret,types[i]);
    }
  }
  ret.append('>');
}
