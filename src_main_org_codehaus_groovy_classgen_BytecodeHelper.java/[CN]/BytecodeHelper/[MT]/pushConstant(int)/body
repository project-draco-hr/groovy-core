{
switch (value) {
case 0:
    cv.visitInsn(ICONST_0);
  break;
case 1:
cv.visitInsn(ICONST_1);
break;
case 2:
cv.visitInsn(ICONST_2);
break;
case 3:
cv.visitInsn(ICONST_3);
break;
case 4:
cv.visitInsn(ICONST_4);
break;
case 5:
cv.visitInsn(ICONST_5);
break;
default :
cv.visitIntInsn(BIPUSH,value);
break;
}
}
