{
  String type=variable.getTypeName();
  int index=variable.getIndex();
  if (holder) {
    cv.visitVarInsn(ALOAD,index);
    cv.visitInsn(SWAP);
    cv.visitMethodInsn(INVOKEVIRTUAL,"groovy/lang/Reference","set","(Ljava/lang/Object;)V");
  }
 else {
    store(variable.deriveBoxedVersion());
  }
}
