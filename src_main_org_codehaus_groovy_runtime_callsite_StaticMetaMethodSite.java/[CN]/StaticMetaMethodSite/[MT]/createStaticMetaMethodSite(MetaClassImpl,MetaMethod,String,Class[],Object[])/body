{
  if (metaMethod.correctArguments(args) == args) {
    if (CallSiteArray.noWrappers(args)) {
      if (CallSiteArray.noCoerce(metaMethod,args))       return new StaticMetaMethodSiteNoUnwrap(name,metaClass,metaMethod,params);
 else       return new StaticMetaMethodSiteNoUnwrapNoCoerce(name,metaClass,metaMethod,params);
    }
  }
  return new StaticMetaMethodSite(name,metaClass,metaMethod,params);
}
