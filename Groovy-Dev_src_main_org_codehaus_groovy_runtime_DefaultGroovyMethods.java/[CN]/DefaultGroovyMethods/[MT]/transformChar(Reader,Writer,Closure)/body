{
  int c;
  try {
    char[] chars=new char[1];
    while ((c=self.read()) != -1) {
      chars[0]=(char)c;
      writer.write((String)closure.call(new String(chars)));
    }
    writer.flush();
    Writer temp2=writer;
    writer=null;
    temp2.close();
    Reader temp1=self;
    self=null;
    temp1.close();
  }
  finally {
    closeReaderWithWarning(self);
    closeWriterWithWarning(writer);
  }
}
