{
  if (c == BigDecimal.class) {
    return toBigDecimal(self);
  }
 else   if (c == BigInteger.class) {
    return toBigInteger(self);
  }
 else   if (c == Double.class) {
    return toDouble(self);
  }
 else   if (c == Float.class) {
    return toFloat(self);
  }
  return asType((Object)self,c);
}
