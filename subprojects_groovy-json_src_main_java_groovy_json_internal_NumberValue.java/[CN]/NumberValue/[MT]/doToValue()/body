{
switch (type) {
case DOUBLE:
    return doubleValue();
case INTEGER:
  int sign=1;
boolean negative=false;
if (buffer[startIndex] == '-') {
startIndex++;
sign=-1;
negative=true;
}
if (isInteger(buffer,startIndex,endIndex - startIndex,negative)) {
return intValue() * sign;
}
 else {
return longValue() * sign;
}
}
die();
return null;
}
