{
  long sign=1;
  if (buffer[startIndex] == '-') {
    startIndex++;
    sign=-1;
  }
  if (isInteger(buffer,startIndex,endIndex - startIndex,sign < 0)) {
    return parseInt(buffer,startIndex,endIndex - startIndex) * sign;
  }
 else {
    return parseLong(buffer,startIndex,endIndex - startIndex) * sign;
  }
}
