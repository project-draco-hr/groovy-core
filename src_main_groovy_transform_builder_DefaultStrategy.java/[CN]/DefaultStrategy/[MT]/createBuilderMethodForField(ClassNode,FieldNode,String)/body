{
  String fieldName=fieldNode.getName();
  String setterName=getSetterName(prefix,fieldName);
  return new MethodNode(setterName,ACC_PUBLIC,newClass(builder),params(param(fieldNode.getType(),fieldName)),NO_EXCEPTIONS,block(stmt(assignX(propX(varX("this"),constX(fieldName)),varX(fieldName))),returnS(varX("this",builder))));
}
