{
  if ("mixed".equalsIgnoreCase(str)) {
    return MIXED;
  }
 else   if ("async".equalsIgnoreCase(str)) {
    return ASYNC;
  }
 else   if ("sync".equalsIgnoreCase(str)) {
    return SYNC;
  }
 else   if ("same".equalsIgnoreCase(str)) {
    return SAME;
  }
 else   if ("outside".equalsIgnoreCase(str)) {
    return OUTSIDE;
  }
 else   if ("defer".equalsIgnoreCase(str)) {
    return DEFER;
  }
  return null;
}
