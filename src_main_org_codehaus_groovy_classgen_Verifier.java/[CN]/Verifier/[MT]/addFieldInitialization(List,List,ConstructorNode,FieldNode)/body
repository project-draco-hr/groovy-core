{
  Expression expression=fieldNode.getInitialValueExpression();
  if (expression != null) {
    ExpressionStatement statement=new ExpressionStatement(new BinaryExpression(new FieldExpression(fieldNode),Token.equal(fieldNode.getLineNumber(),fieldNode.getColumnNumber()),expression));
    if (fieldNode.isStatic()) {
      staticList.add(statement);
    }
 else {
      list.add(statement);
    }
  }
}
