{
  Token[] tokens=new Token[]{Token.leftParenthesis(1,1),Token.rightParenthesis(1,2),Token.leftSquareBracket(1,3),Token.rightSquareBracket(1,4),Token.leftCurlyBrace(1,5),Token.rightCurlyBrace(1,6)};
  MockTokenStream in=new MockTokenStream(tokens);
  assertSame(tokens[0],in.la());
  assertSame(tokens[1],in.la(2));
  assertSame(tokens[2],in.la(3));
  assertSame(tokens[3],in.la(4));
  assertSame(tokens[4],in.la(5));
  try {
    in.la(6);
    fail("should have thrown LookAheadExhaustionError");
  }
 catch (  LookAheadExhaustionError e) {
    assertEquals(6,e.getLookAhead());
  }
}
