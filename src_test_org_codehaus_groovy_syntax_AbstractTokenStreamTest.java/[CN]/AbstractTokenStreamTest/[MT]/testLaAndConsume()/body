{
  Token[] tokens=new Token[]{Token.newSymbol("(",1,1),Token.newSymbol(")",1,2)};
  MockTokenStream in=new MockTokenStream(tokens);
  assertSame(tokens[0],in.la());
  assertSame(tokens[0],in.la());
  assertSame(tokens[0],in.la(1));
  assertSame(tokens[0],in.la(1));
  assertSame(tokens[1],in.la(2));
  in.consume(Types.LEFT_PARENTHESIS);
  assertSame(tokens[1],in.la());
  assertSame(tokens[1],in.la());
  assertSame(tokens[1],in.la(1));
  assertSame(tokens[1],in.la(1));
}
