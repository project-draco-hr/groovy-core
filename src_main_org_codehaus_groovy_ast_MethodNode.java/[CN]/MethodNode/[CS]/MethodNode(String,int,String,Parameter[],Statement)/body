{
  this.name=name;
  this.modifiers=modifiers;
  this.parameters=parameters;
  this.code=code;
  if (returnType == null || returnType.length() == 0) {
    this.returnType="java.lang.Object";
    this.dynamicReturnType=true;
  }
 else {
    this.returnType=ensureJavaTypeNameSyntax(returnType);
  }
}
