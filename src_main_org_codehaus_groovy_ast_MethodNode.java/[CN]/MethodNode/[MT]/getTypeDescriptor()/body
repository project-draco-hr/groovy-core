{
  StringBuffer buf=new StringBuffer();
  buf.append(dynamicReturnType ? "$dynamic" : returnType);
  buf.append(' ');
  buf.append(name);
  buf.append('(');
  for (int i=0; i < parameters.length; i++) {
    if (i > 0)     buf.append(',');
    Parameter param=parameters[i];
    buf.append(ensureJavaTypeNameSyntax(param.getType()));
  }
  buf.append(')');
  return buf.toString();
}
