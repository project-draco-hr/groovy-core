{
  if (typename.charAt(0) == '[') {
    return cleanupTypeName(typename.substring(1)) + "[]";
  }
  if (typename.length() == 1) {
switch (typename.charAt(0)) {
case 'J':
      return "long";
case 'I':
    return "int";
case 'C':
  return "char";
case 'F':
return "float";
case 'V':
return "void";
case 'D':
return "double";
}
}
if (typename.endsWith(";")) {
return typename.substring(1,typename.length() - 1);
}
return typename;
}
