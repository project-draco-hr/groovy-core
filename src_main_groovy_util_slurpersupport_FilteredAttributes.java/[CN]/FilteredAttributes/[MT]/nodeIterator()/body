{
  return new NodeIterator(this.parent.iterator()){
    protected Object getNextNode(    final Iterator iter){
      while (iter.hasNext()) {
        final Object node=iter.next();
        if (InvokerHelper.asBool(FilteredAttributes.this.closure.call(new Object[]{node}))) {
          return node;
        }
      }
      return null;
    }
  }
;
}
