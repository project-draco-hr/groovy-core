{
  return new IfStatement(new NotExpression(new MethodCallExpression(new VariableExpression("field",FIELD_TYPE),"isSynthetic",MethodCallExpression.NO_ARGUMENTS)),new BlockStatement(new Statement[]{AbstractASTTransformUtil.declStatement(new VariableExpression("newValue",ClassHelper.OBJECT_TYPE),new MethodCallExpression(new VariableExpression("map",ClassHelper.MAP_TYPE),"get",new MethodCallExpression(new VariableExpression("field",FIELD_TYPE),"getName",MethodCallExpression.NO_ARGUMENTS))),createCallGetterForField(cNode),createNewValueIf(),createAddToConstructMap()},new VariableScope()),EmptyStatement.INSTANCE);
}
