{
  if (getMemberValue(anno,memberName) != null) {
    String tname=TupleConstructorASTTransformation.MY_TYPE_NAME;
    addError("Error during " + MY_TYPE_NAME + " processing: Annotation attribute '"+ memberName+ "' not supported for "+ tname+ " when used with "+ MY_TYPE_NAME,anno);
    return true;
  }
  return false;
}
