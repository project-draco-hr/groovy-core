{
  return AbstractASTTransformUtil.declStatement(new VariableExpression("value",ClassHelper.OBJECT_TYPE),new MethodCallExpression(new MethodCallExpression(new ClassExpression(cNode),"getMethod",new ArgumentListExpression(new Expression[]{new BinaryExpression(new ConstantExpression("get"),new Token(Types.PLUS,"+",-1,-1),new BinaryExpression(new MethodCallExpression(new MethodCallExpression(new MethodCallExpression(new VariableExpression("field",FIELD_TYPE),"getName",MethodCallExpression.NO_ARGUMENTS),"substring",new ArgumentListExpression(new Expression[]{new ConstantExpression(0),new ConstantExpression(1)})),"toUpperCase",MethodCallExpression.NO_ARGUMENTS),new Token(Types.PLUS,"+",-1,-1),new MethodCallExpression(new MethodCallExpression(new VariableExpression("field",FIELD_TYPE),"getName",MethodCallExpression.NO_ARGUMENTS),"substring",new ArgumentListExpression(new Expression[]{new ConstantExpression(1)}))))})),"invoke",new VariableExpression("this",cNode)));
}
