{
  return new ExpressionStatement(new MethodCallExpression(new VariableExpression("construct",HASHMAP_TYPE),"put",new ArgumentListExpression(new Expression[]{new MethodCallExpression(new VariableExpression("field",FIELD_TYPE),"getName",MethodCallExpression.NO_ARGUMENTS),new VariableExpression("value",ClassHelper.OBJECT_TYPE)})));
}
