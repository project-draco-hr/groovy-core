{
  Expression expression=createIfInstanceOfAsImmutableS(fieldExpr,SORTEDSET_CLASSNODE,createIfInstanceOfAsImmutableS(fieldExpr,SORTEDMAP_CLASSNODE,createIfInstanceOfAsImmutableS(fieldExpr,SET_CLASSNODE,createIfInstanceOfAsImmutableS(fieldExpr,MAP_CLASSNODE,createIfInstanceOfAsImmutableS(fieldExpr,ClassHelper.LIST_TYPE,createAsImmutableX(fieldExpr,COLLECTION_TYPE))))));
  return expression;
}
