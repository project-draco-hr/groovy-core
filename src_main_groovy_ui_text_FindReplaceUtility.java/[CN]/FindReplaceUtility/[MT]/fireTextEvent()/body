{
  EventListener[] lstrs=eventListenerList.getListeners(TextListener.class);
  if (lstrs != null && lstrs.length > 0) {
    TextEvent te=new TextEvent(findReplaceDialog,TextEvent.TEXT_VALUE_CHANGED);
    for (int i=0; i < lstrs.length; i++) {
      ((TextListener)lstrs[i]).textValueChanged(te);
    }
  }
}
