{
  if (arguments == null) {
    return "null";
  }
  String sbdry="{";
  String ebdry="}";
  StringBuffer argBuf=new StringBuffer(sbdry);
  for (int i=0; i < arguments.length; i++) {
    if (i > 0) {
      argBuf.append(", ");
    }
    argBuf.append(format(arguments[i],true));
  }
  argBuf.append(ebdry);
  return argBuf.toString();
}
