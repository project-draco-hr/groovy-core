{
  if (left == right) {
    return true;
  }
  if (left != null) {
    if (right == null) {
      return false;
    }
    if (left instanceof Number) {
      return DefaultGroovyMethods.compareTo((Number)left,asNumber(right)) == 0;
    }
    if (left.equals(right)) {
      return true;
    }
  }
  return false;
}
