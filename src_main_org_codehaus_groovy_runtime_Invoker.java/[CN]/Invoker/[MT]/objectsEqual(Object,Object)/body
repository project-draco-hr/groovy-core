{
  if (left == right) {
    return true;
  }
  if (left != null) {
    if (right == null) {
      return false;
    }
    if (left instanceof Comparable) {
      return compareTo(left,right) == 0;
    }
 else     if (left instanceof List && right instanceof List) {
      return DefaultGroovyMethods.equals((List)left,(List)right);
    }
 else {
      return left.equals(right);
    }
  }
  return false;
}
