{
  if (loadDefault == LOAD_DEFAULT) {
    this.useAccessible=true;
    ((MetaClassImpl)lookup(DefaultGroovyMethods.class)).registerInstanceMethods();
    ((MetaClassImpl)lookup(DefaultGroovyStaticMethods.class)).registerStaticMethods();
    checkInitialised();
  }
 else {
    this.useAccessible=true;
  }
}
