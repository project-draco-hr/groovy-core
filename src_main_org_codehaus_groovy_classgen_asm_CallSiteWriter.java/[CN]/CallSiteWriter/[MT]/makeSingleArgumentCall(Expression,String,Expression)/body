{
  OperandStack operandStack=controller.getOperandStack();
  int m1=operandStack.getStackLength();
  prepareSiteAndReceiver(receiver,message,false,controller.getCompileStack().isLHS());
  visitBoxedArgument(arguments);
  int m2=operandStack.getStackLength();
  controller.getMethodVisitor().visitMethodInsn(INVOKEINTERFACE,"org/codehaus/groovy/runtime/callsite/CallSite","call","(Ljava/lang/Object;Ljava/lang/Object;)Ljava/lang/Object;");
  operandStack.replace(ClassHelper.OBJECT_TYPE,m2 - m1);
}
