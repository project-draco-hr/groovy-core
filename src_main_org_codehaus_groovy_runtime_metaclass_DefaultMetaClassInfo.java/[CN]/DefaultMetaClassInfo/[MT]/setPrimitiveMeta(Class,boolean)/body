{
  if (c == Byte.class) {
    setOrigByte(orig);
  }
 else   if (c == Character.class) {
    setOrigChar(orig);
  }
 else   if (c == Short.class) {
    setOrigShort(orig);
  }
 else   if (c == Integer.class) {
    setOrigInt(orig);
  }
 else   if (c.getComponentType() == Integer.class) {
    setOrigIntArray(orig);
  }
 else   if (c == Long.class) {
    setOrigLong(orig);
  }
 else   if (c == Float.class) {
    setOrigFloat(orig);
  }
 else   if (c == Double.class) {
    setOrigDouble(orig);
  }
}
