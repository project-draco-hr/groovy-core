{
  CSTNode expr=primaryExpression();
  Token laToken=la();
  if (laToken != null) {
switch (laToken.getType()) {
case (Token.PLUS_PLUS):
case (Token.MINUS_MINUS):
{
        CSTNode primaryExpr=expr;
        expr=new CSTNode(Token.syntheticPostfix());
        expr.addChild(rootNode(lt(),primaryExpr));
      }
  }
}
return expr;
}
