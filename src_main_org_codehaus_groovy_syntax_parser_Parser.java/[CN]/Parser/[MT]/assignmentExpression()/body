{
  CSTNode expr=conditionalExpression();
switch (lt()) {
case (Token.EQUAL):
case (Token.PLUS_EQUAL):
case (Token.MINUS_EQUAL):
case (Token.DIVIDE_EQUAL):
case (Token.MULTIPLY_EQUAL):
case (Token.MOD_EQUAL):
{
      rootNode(lt(),expr);
      expr.addChild(conditionalExpression());
      break;
    }
}
return expr;
}
