{
  Token leading=(useBare ? la_bare() : la());
  while ((useBare ? lt_bare() : lt()) != -1 && !(useBare ? la_bare() : la()).isA(safe)) {
    if (useBare) {
      consume_bare(lt_bare());
    }
 else {
      consume(lt());
    }
  }
  if ((useBare ? la_bare() : la()) == leading) {
    if (useBare) {
      consume_bare(lt_bare());
    }
 else {
      consume(lt());
    }
  }
}
