{
  CSTNode parameterDeclaration=null;
switch (lt(2)) {
case (Token.IDENTIFIER):
case (Token.DOT):
{
      parameterDeclaration=parameterDeclarationWithDatatype();
      break;
    }
default :
{
    parameterDeclaration=parameterDeclarationWithoutDatatype();
    break;
  }
}
return parameterDeclaration;
}
