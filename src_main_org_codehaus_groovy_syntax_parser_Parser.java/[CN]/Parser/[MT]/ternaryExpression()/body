{
  CSTNode expr=logicalOrExpression();
  if (lt_bare() == Token.QUESTION) {
    expr=rootNode(Token.QUESTION,expr);
    optionalNewlines();
    expr.addChild(assignmentExpression());
    optionalNewlines();
    consume(Token.COLON);
    optionalNewlines();
    expr.addChild(ternaryExpression());
  }
  return expr;
}
