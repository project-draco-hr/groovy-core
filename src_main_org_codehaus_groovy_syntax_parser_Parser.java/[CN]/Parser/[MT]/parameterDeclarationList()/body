{
  CSTNode parameterDeclarationList=new CSTNode();
  while (lt() == Token.IDENTIFIER || isIdentifierOrPrimtiveTypeKeyword(lt())) {
    parameterDeclarationList.addChild(parameterDeclaration());
    if (lt() == Token.COMMA) {
      consume(Token.COMMA);
    }
 else {
      break;
    }
  }
  return parameterDeclarationList;
}
