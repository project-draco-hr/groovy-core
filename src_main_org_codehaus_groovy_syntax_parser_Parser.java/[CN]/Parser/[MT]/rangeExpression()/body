{
  CSTNode expr=additiveExpression();
  if (lt_bare() == Token.DOT_DOT) {
    expr=rootNode(Token.DOT_DOT,expr);
    optionalNewlines();
    expr.addChild(additiveExpression());
  }
 else   if (lt_bare() == Token.DOT_DOT_DOT) {
    expr=rootNode(Token.DOT_DOT_DOT,expr);
    optionalNewlines();
    expr.addChild(additiveExpression());
  }
  return expr;
}
