{
  if (cachedSuperClass == null) {
    if (!isArray)     cachedSuperClass=ReflectionCache.getCachedClass(getTheClass().getSuperclass());
 else     if (cachedClass.getComponentType().isPrimitive() || cachedClass.getComponentType() == Object.class)     cachedSuperClass=ReflectionCache.OBJECT_CLASS;
 else     cachedSuperClass=ReflectionCache.OBJECT_ARRAY_CLASS;
  }
  return cachedSuperClass;
}
