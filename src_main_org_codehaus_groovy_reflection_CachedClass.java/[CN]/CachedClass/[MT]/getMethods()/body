{
  if (methods == null) {
    final Method[] declaredMethods=getCachedClass().getDeclaredMethods();
    methods=new CachedMethod[declaredMethods.length];
    for (int i=0; i != methods.length; ++i)     methods[i]=new CachedMethod(this,declaredMethods[i]);
    Arrays.sort(methods);
  }
  return methods;
}
