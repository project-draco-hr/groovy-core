{
  try {
    return getMetaClass().invokeMethod(this,method,arguments);
  }
 catch (  InvokerException e) {
    Object delegate=getDelegate();
    if (delegate != this) {
      try {
        return InvokerHelper.invokeMethod(delegate,method,arguments);
      }
 catch (      InvokerException e2) {
      }
    }
    throw e;
  }
}
