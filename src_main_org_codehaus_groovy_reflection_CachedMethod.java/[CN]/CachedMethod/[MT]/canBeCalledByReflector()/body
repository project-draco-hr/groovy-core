{
  if (!Modifier.isPublic(cachedClass.getModifiers()))   return false;
  if (!Modifier.isPublic(getModifiers()))   return false;
  getParameterTypes();
  for (int i=0; i != parameterTypes.length; ++i) {
    if (!parameterTypes[i].isPrimitive && !Modifier.isPublic(parameterTypes[i].getModifiers()))     return false;
  }
  return true;
}
