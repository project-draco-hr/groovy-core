{
  CachedClass[] parameters=getParameterTypes();
  int size=parameters.length;
  for (int i=0; i < size; i++) {
    mv.visitVarInsn(ALOAD,argumentIndex + i);
    Class type=parameters[i].getTheClass();
    if (type.isPrimitive()) {
      helper.unbox(type);
    }
 else {
      helper.doCast(type);
    }
  }
}
