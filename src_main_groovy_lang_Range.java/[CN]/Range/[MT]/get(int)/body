{
  if (index < 0) {
    throw new IndexOutOfBoundsException("Index: " + index + " should not be negative");
  }
  Object value=from;
  for (int i=0; i < index; i++) {
    value=increment(value);
  }
  if (index >= size()) {
    throw new IndexOutOfBoundsException("Index: " + index + " is too big for range: "+ this);
  }
  return value;
}
