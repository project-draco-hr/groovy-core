{
  if (f.isDirectory()) {
    String files[]=f.list();
    for (int i=0; i < files.length; i++)     doFile(new File(f,files[i]));
  }
 else   if (f.getName().endsWith(".groovy")) {
    System.err.println(" --- " + f.getAbsolutePath());
    parseFile(f.getName(),new BufferedReader(new FileReader(f)));
  }
}
