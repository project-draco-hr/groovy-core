{
  Class answer=null;
  try {
    answer=parseClass(new InputStreamCharStream(in));
  }
 catch (  SyntaxException e) {
    try {
      in.close();
    }
 catch (    Exception hide) {
    }
    throw e;
  }
catch (  IOException e) {
    try {
      in.close();
    }
 catch (    Exception hide) {
    }
    throw e;
  }
  in.close();
  return answer;
}
