{
  this.namespaceURI=(namespaceURI == null) ? EMPTY_STRING : namespaceURI;
  if (localPart == null) {
    throw new IllegalArgumentException("invalid QName local part");
  }
 else {
    this.localPart=localPart;
  }
  if (prefix == null) {
    throw new IllegalArgumentException("invalid QName prefix");
  }
 else {
    this.prefix=prefix;
  }
}
