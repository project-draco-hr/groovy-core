{
  int sum=0;
  String line;
  try {
    BufferedReader in=new BufferedReader(new InputStreamReader(System.in));
    while ((line=in.readLine()) != null) {
      sum=sum + Integer.parseInt(line);
    }
  }
 catch (  IOException e) {
    System.err.println(e);
    return;
  }
  System.out.println(Integer.toString(sum));
}
