{
  try {
    return receiver.getClass() == metaClass.getTheClass() && checkMetaClass() && MetaClassHelper.sameClass(params,arg) ? this : createCallSite(receiver,new Object[]{arg});
  }
 catch (  NullPointerException e) {
    if (receiver == null)     return acceptBinop(NullObject.getNullObject(),arg);
    throw e;
  }
}
