{
  try {
    return !GroovyCategorySupport.hasCategoryInAnyThread() && receiver.getClass() == metaClass.getTheClass() && ((MetaClassImpl)metaClass).getTheCachedClass().getMetaClassForClass() == metaClass && MetaClassHelper.sameClasses(params,args);
  }
 catch (  NullPointerException e) {
    if (receiver == null)     return checkAcceptCall(NullObject.getNullObject(),args);
    throw e;
  }
}
