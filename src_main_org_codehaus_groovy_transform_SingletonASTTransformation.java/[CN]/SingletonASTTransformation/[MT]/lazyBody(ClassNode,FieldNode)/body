{
  final Expression instanceExpression=new VariableExpression(fieldNode);
  return new IfStatement(new BooleanExpression(new BinaryExpression(instanceExpression,Token.newSymbol("!=",-1,-1),ConstantExpression.NULL)),new ReturnStatement(instanceExpression),new SynchronizedStatement(new ClassExpression(classNode),new IfStatement(new BooleanExpression(new BinaryExpression(instanceExpression,Token.newSymbol("!=",-1,-1),ConstantExpression.NULL)),new ReturnStatement(instanceExpression),new ReturnStatement(new BinaryExpression(instanceExpression,Token.newSymbol("=",-1,-1),new ConstructorCallExpression(classNode,new ArgumentListExpression()))))));
}
