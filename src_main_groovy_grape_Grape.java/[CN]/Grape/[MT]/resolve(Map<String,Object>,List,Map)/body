{
  URI[] uris=null;
  if (enableGrapes) {
    GrapeEngine instance=getInstance();
    if (instance != null) {
      if (!args.containsKey(AUTO_DOWNLOAD_SETTING)) {
        args.put(AUTO_DOWNLOAD_SETTING,enableAutoDownload);
      }
      uris=instance.resolve(args,depsInfo,dependencies);
    }
  }
  if (uris == null) {
    return new URI[0];
  }
 else {
    return uris;
  }
}
