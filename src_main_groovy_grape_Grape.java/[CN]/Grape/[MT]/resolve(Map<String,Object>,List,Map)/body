{
  URI[] uris=null;
  if (enableGrapes) {
    GrapeEngine instance=getInstance();
    if (instance != null) {
      if (!args.containsKey("autoDownload")) {
        args.put("autoDownload",enableAutoDownload);
      }
      uris=instance.resolve(args,depsInfo,dependencies);
    }
  }
  if (uris == null) {
    return new URI[0];
  }
 else {
    return uris;
  }
}
