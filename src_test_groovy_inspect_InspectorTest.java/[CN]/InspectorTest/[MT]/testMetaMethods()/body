{
  Inspector insp=new Inspector(new Object());
  String[][] metaMethods=insp.getMetaMethods();
  assertEquals(28,metaMethods.length);
  String[] names={"println","println","println","find","print","print","each","invokeMethod","inspect","isCase","identity","getAt","putAt","dump","eachPropertyName","eachProperty","allProperties","use","use","printf","eachWithIndex","every","any","grep","collect","collect","findAll","findIndexOf"};
  assertNameEquals(names,metaMethods);
  String[] details={"GROOVY","public","Object","void","println","Object",""};
  assertContains(metaMethods,details);
}
