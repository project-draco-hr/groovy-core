{
switch (nibble) {
case 0x00:
case 0x01:
case 0x02:
case 0x03:
case 0x04:
case 0x05:
case 0x06:
case 0x07:
case 0x08:
case 0x09:
    return nibble + 0x30;
case 0x0A:
case 0x0B:
case 0x0C:
case 0x0D:
case 0x0E:
case 0x0F:
  return nibble + 0x57;
default :
die("illegal nibble: " + nibble);
return -1;
}
}
