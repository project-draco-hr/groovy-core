{
  if (sql == null) {
    sql="select * from " + table;
    if (where != null) {
      String clause="";
      if (parent != null && parent.where != null) {
        clause+=parent.getSqlVisitor().getWhere() + " and ";
      }
      clause+=getSqlVisitor().getWhere();
      if (clause.length() > 0) {
        sql+=" where " + clause;
      }
    }
  }
  return sql;
}
