{
  sw.write("\");out.print(\"${");
  int c;
  while ((c=reader.read()) != -1) {
    if (c == '%') {
      c=reader.read();
      if (c != '>') {
        sw.write('%');
      }
 else {
        break;
      }
    }
    if (c != '\n' && c != '\r') {
      sw.write(c);
    }
  }
  sw.write("}\");\nout.print(\"");
}
