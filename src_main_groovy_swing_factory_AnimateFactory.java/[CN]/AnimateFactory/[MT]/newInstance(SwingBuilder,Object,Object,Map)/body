{
  if (value == null) {
    value=properties.remove("range");
  }
  FullBinding fb=null;
  if (value instanceof List) {
    fb=createListAnimation(builder,(List)value,properties);
  }
  if (fb != null) {
    Object o=properties.remove("start");
    if (o == null || ((o instanceof Boolean) && ((Boolean)o).booleanValue()) || ((o instanceof String) && Boolean.valueOf((String)o).booleanValue())) {
      fb.bind();
    }
  }
  return fb;
}
