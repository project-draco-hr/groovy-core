{
  if (!(args[0] instanceof Integer) || !(args[1] instanceof Long))   return PojoMetaMethodSite.createNonAwareCallSite(site,metaClass,metaMethod,params,args);
 else   return new PojoMetaMethodSite(site,metaClass,metaMethod,params){
    public Object call(    Object receiver,    Object[] args){
      if ((receiver instanceof long[] && args[0] instanceof Integer && args[1] instanceof Long) && checkPojoMetaClass()) {
        final long[] objects=(long[])receiver;
        objects[normaliseIndex(((Integer)args[0]).intValue(),objects.length)]=((Long)args[1]).longValue();
        return null;
      }
 else       return super.call(receiver,args);
    }
  }
;
}
