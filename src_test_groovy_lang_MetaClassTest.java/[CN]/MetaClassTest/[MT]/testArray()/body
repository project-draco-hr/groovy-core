{
  try {
    String[] value=new String[]{"hello"};
    MetaClass metaClass=InvokerHelper.getMetaClass(value);
    assertTrue("got metaclass",metaClass != null);
    metaClass.invokeMethod(value,"toString",new Object[0]);
  }
 catch (  Exception e) {
    System.out.println("Caught: " + e);
    e.printStackTrace();
  }
}
