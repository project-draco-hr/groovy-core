{
  Long err=((long)expr.getLineNumber()) << 16 + expr.getColumnNumber();
  if (!typeCheckingContext.reportedErrors.contains(err)) {
    typeCheckingContext.errorCollector.addErrorAndContinue(new SyntaxErrorMessage(new SyntaxException(msg + '\n' + typeCheckingContext.getEnclosingClosureStack(),expr.getLineNumber(),expr.getColumnNumber(),expr.getLastLineNumber(),expr.getLastColumnNumber()),typeCheckingContext.source));
    typeCheckingContext.reportedErrors.add(err);
  }
}
