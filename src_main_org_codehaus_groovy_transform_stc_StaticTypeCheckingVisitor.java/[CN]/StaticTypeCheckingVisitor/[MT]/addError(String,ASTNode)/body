{
  Long err=((long)expr.getLineNumber()) << 16 + expr.getColumnNumber();
  if (!reportedErrors.contains(err)) {
    errorCollector.addErrorAndContinue(new SyntaxErrorMessage(new SyntaxException(msg + '\n',expr.getLineNumber(),expr.getColumnNumber(),expr.getLastLineNumber(),expr.getLastColumnNumber()),source));
    reportedErrors.add(err);
  }
}
