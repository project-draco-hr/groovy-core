{
  if (nodes == null)   return "[]";
  StringBuilder sb=new StringBuilder(24 * nodes.length);
  sb.append("[");
  for (  ClassNode node : nodes) {
    sb.append(prettyPrintType(node));
    sb.append(", ");
  }
  if (sb.length() > 1) {
    sb.setCharAt(sb.length() - 2,']');
  }
  return sb.toString();
}
