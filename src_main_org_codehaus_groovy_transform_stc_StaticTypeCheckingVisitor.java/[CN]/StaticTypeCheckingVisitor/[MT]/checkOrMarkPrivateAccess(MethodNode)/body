{
  if (mn != null && Modifier.isPrivate(mn.getModifiers()) && (mn.getDeclaringClass() != typeCheckingContext.getEnclosingClassNode() || typeCheckingContext.getEnclosingClosure() != null) && mn.getDeclaringClass().getModule() == typeCheckingContext.getEnclosingClassNode().getModule()) {
    addPrivateFieldOrMethodAccess(mn.getDeclaringClass(),StaticTypesMarker.PV_METHODS_ACCESS,mn);
  }
}
