{
  if (mn != null && Modifier.isPrivate(mn.getModifiers()) && (mn.getDeclaringClass() != classNode || closureExpression != null) && mn.getDeclaringClass().getModule() == classNode.getModule()) {
    addPrivateFieldOrMethodAccess(mn.getDeclaringClass(),StaticTypesMarker.PV_METHODS_ACCESS,mn);
  }
}
