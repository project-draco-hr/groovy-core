{
  if (byte_TYPE.equals(type) || short_TYPE.equals(type) || int_TYPE.equals(type)) {
    return int_TYPE;
  }
  if (Byte_TYPE.equals(type) || Short_TYPE.equals(type) || Integer_TYPE.equals(type)) {
    return Integer_TYPE;
  }
  if (float_TYPE.equals(type))   return double_TYPE;
  if (Float_TYPE.equals(type))   return Double_TYPE;
  return type;
}
