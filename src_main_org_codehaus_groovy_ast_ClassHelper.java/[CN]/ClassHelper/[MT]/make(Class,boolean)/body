{
  for (int i=0; i < classes.length; i++) {
    if (c == classes[i])     return types[i];
  }
  if (c.isArray()) {
    ClassNode cn=make(c.getComponentType());
    return cn.makeArray();
  }
  ClassNode t=new ClassNode(c);
  if (includeGenerics)   VMPluginFactory.getPlugin().setGenericsTypes(t);
  return t;
}
