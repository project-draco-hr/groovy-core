{
  SwingBuilder.checkValueIsNull(value,name);
  if (properties.isEmpty()) {
    return new ArrayList();
  }
 else {
    Map.Entry item=(Map.Entry)properties.entrySet().iterator().next();
    throw new MissingPropertyException("The builder element '" + name + "' is a collections element and has no properties",item.getKey().toString(),item.getValue().getClass());
  }
}
