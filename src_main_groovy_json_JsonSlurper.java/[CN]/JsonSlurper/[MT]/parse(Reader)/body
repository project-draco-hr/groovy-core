{
  Object content;
  JsonLexer lexer=new JsonLexer(reader);
  JsonToken token=lexer.nextToken();
  if (token.getType() == OPEN_CURLY) {
    content=parseObject(lexer);
  }
 else   if (token.getType() == OPEN_BRACKET) {
    content=parseArray(lexer);
  }
 else {
    throw new JsonException("A JSON payload should start with ${OPEN_CURLY.label} or ${OPEN_BRACKET.label}.\n" + "Instead, '${token.text}' was found on line: ${token.startLine}, column: ${token.startColumn}");
  }
  return content;
}
