{
  StringCharStream charStream=new StringCharStream("cheese");
  assertEquals('c',charStream.consume());
  assertEquals('h',charStream.consume());
  assertEquals('e',charStream.consume());
  assertEquals('e',charStream.consume());
  assertEquals('s',charStream.consume());
  assertEquals('e',charStream.consume());
  assertEquals(CharStream.EOS,charStream.consume());
  assertEquals(CharStream.EOS,charStream.consume());
}
