{
  boolean valid=true;
  final int length=to - start;
  if (length != JSON_TIME_LENGTH) {
    return false;
  }
  valid&=(charArray[start + 19] == '.');
  if (!valid) {
    return false;
  }
  valid&=(charArray[start + 4] == '-') && (charArray[start + 7] == '-') && (charArray[start + 10] == 'T')&& (charArray[start + 13] == ':')&& (charArray[start + 16] == ':');
  return valid;
}
