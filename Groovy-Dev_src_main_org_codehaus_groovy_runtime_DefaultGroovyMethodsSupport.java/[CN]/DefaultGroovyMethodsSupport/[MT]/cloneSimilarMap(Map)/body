{
  Map answer=(Map)cloneObject(orig);
  if (answer != null)   return answer;
  answer=cloneMapFromClass(orig);
  if (answer != null)   return answer;
  if (orig instanceof TreeMap)   return new TreeMap(orig);
  if (orig instanceof LinkedHashMap)   return new LinkedHashMap(orig);
  if (orig instanceof Properties) {
    Map map=new Properties();
    map.putAll(orig);
    return map;
  }
  if (orig instanceof Hashtable)   return new Hashtable(orig);
  return new HashMap(orig);
}
