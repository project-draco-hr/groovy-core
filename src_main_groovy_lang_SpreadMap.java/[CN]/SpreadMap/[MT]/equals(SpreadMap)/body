{
  if (that == null)   return false;
  if (size() == that.size()) {
    SpreadMap other=(SpreadMap)that;
    Iterator iter=mapData.keySet().iterator();
    for (; iter.hasNext(); ) {
      Object key=iter.next();
      if (!DefaultTypeTransformation.compareEqual(get(key),other.get(key))) {
        return false;
      }
    }
    return true;
  }
  return false;
}
