{
  if (mapData.isEmpty()) {
    return "*:[:]";
  }
  StringBuffer buff=new StringBuffer("*:[");
  Iterator iter=mapData.keySet().iterator();
  for (; iter.hasNext(); ) {
    Object key=iter.next();
    buff.append(key + ":" + mapData.get(key));
    if (iter.hasNext())     buff.append(", ");
  }
  buff.append("]");
  return buff.toString();
}
