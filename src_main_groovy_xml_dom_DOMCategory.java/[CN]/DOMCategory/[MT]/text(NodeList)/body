{
  List result=new ArrayList();
  for (int i=0; i < nodeList.getLength(); i++) {
    Node node=nodeList.item(i);
    if (!(node instanceof Element))     continue;
    Element element=(Element)node;
    String text=text(element);
    if (text != null) {
      result.add(text);
    }
  }
  return result;
}
