{
  for (int i=0; i < array.length; ++i) {
    if (array[i] == expected) {
      return;
    }
  }
  StringBuffer message=new StringBuffer();
  message.append(expected + " not in {");
  for (int i=0; i < array.length; ++i) {
    message.append("'" + array[i] + "'");
    if (i < (array.length - 1)) {
      message.append(", ");
    }
  }
  message.append(" }");
  fail(message.toString());
}
