{
  MethodNode mNode=new MethodNode(methodNode.getName(),ACC_PUBLIC,methodNode.getReturnType(),methodNode.getParameters(),methodNode.getExceptions(),processBody(methodNode.getCode()));
  if (methodNode.getCode() == null) {
    methodNode.addAnnotation(new AnnotationNode(ABSTRACT_METHOD));
  }
 else {
    methodNode.setCode(null);
  }
  methodNode.setModifiers(ACC_PUBLIC | ACC_ABSTRACT);
  return mNode;
}
