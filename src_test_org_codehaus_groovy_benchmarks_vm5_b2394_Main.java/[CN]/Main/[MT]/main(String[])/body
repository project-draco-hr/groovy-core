{
  if (args == null || args.length != 3) {
    for (int i=1; i <= 50; ) {
      new Main("script300.groovy",(5000 * 20) / i,i);
      if (i < 10)       i++;
 else       if (i < 20) {
        i+=2;
      }
 else {
        i+=5;
      }
    }
  }
 else {
    new Main(args[0],Integer.parseInt(args[1]),Integer.parseInt(args[2]));
  }
}
