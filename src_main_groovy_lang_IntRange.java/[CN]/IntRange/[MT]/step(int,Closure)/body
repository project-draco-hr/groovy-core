{
  if (reverse) {
    step=-step;
  }
  if (step >= 0) {
    int value=from;
    while (value <= to) {
      closure.call(Integer.valueOf(value));
      value=value + step;
    }
  }
 else {
    int value=to;
    while (value >= from) {
      closure.call(Integer.valueOf(value));
      value=value + step;
    }
  }
}
