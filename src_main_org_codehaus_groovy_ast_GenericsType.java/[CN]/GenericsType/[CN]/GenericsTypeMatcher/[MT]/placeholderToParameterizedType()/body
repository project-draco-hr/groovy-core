{
  Map<String,ClassNode> result=new HashMap<String,ClassNode>();
  collectParameter(type,result);
  if (upperBounds != null) {
    for (    ClassNode upperBound : upperBounds) {
      collectParameter(upperBound,result);
    }
  }
  if (lowerBound != null) {
    collectParameter(lowerBound,result);
  }
  return result;
}
