{
  cw=new ClassWriter(false);
  String fileName=className;
  int idx=className.lastIndexOf('.');
  if (idx > 0) {
    fileName=className.substring(idx + 1);
  }
  fileName+=".java";
  cw.visit(ACC_PUBLIC + ACC_SUPER,helper.getClassInternalName(className),"org/codehaus/groovy/runtime/Reflector",null,fileName);
  cv=cw.visitMethod(ACC_PUBLIC,"<init>","()V",null,null);
  cv.visitVarInsn(ALOAD,0);
  cv.visitMethodInsn(INVOKESPECIAL,"org/codehaus/groovy/runtime/Reflector","<init>","()V");
  cv.visitInsn(RETURN);
  cv.visitMaxs(1,1);
  cw.visitEnd();
  return cw.toByteArray();
}
