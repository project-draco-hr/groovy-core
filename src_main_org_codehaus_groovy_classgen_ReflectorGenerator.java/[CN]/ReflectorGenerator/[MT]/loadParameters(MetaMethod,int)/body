{
  Class[] parameters=method.getParameterTypes();
  int size=parameters.length;
  for (int i=0; i < size; i++) {
    cv.visitVarInsn(ALOAD,argumentIndex);
    helper.pushConstant(i);
    cv.visitInsn(AALOAD);
    Class type=parameters[i];
    if (type.isPrimitive()) {
      helper.unbox(type);
    }
 else {
      helper.doCast(type);
    }
  }
}
