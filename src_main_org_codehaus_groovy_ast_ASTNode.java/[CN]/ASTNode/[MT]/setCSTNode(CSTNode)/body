{
  Token token=node.getRoot();
  if (token != null) {
    setColumnNumber(token.getStartColumn());
    setLineNumber(token.getStartLine());
  }
  if (getLineNumber() < 0) {
    if (node.children() > 0) {
      for (int i=1; i < node.size(); i++) {
        CSTNode child=node.get(i);
        if (child != null) {
          setCSTNode(child);
          if (getLineNumber() >= 0) {
            break;
          }
        }
      }
    }
  }
}
