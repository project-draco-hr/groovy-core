{
  Expression expression=expressionSwitch(node);
  if (convertToConstant) {
    if (expression != VariableExpression.THIS_EXPRESSION && expression != VariableExpression.SUPER_EXPRESSION && expression instanceof VariableExpression) {
      VariableExpression ve=(VariableExpression)expression;
      expression=new ConstantExpression(ve.getName());
    }
  }
  configureAST(expression,node);
  return expression;
}
