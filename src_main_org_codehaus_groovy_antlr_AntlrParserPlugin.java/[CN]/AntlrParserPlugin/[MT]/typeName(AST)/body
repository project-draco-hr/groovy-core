{
  String answer=null;
  AST node=typeNode.getFirstChild();
  if (node != null) {
    if (isType(INDEX_OP,node) || isType(ARRAY_DECLARATOR,node)) {
      return resolveTypeName(node.getFirstChild().getText()) + "[]";
    }
    answer=resolveTypeName(node.getText());
    node=node.getNextSibling();
    if (isType(INDEX_OP,node) || isType(ARRAY_DECLARATOR,node)) {
      return answer + "[]";
    }
  }
  return answer;
}
