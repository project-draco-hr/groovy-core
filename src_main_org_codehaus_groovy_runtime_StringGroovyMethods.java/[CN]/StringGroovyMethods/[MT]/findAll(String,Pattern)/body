{
  Matcher matcher=pattern.matcher(self);
  List<String> list=new ArrayList<String>();
  for (Iterator iter=iterator(matcher); iter.hasNext(); ) {
    if (hasGroup(matcher)) {
      list.add((String)((List)iter.next()).get(0));
    }
 else {
      list.add((String)iter.next());
    }
  }
  return list;
}
