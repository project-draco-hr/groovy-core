{
  if (index < 0)   throw new ArrayIndexOutOfBoundsException(index);
  final Iterator iter=iterator();
  int count=0;
  while (iter.hasNext()) {
    if (count++ == index) {
      return iter.next();
    }
 else {
      iter.next();
    }
  }
  return new NoChildren(this,this.name,this.namespaceTagHints);
}
