{
  MethodCallExpression conversion=new MethodCallExpression(expr.isImplicitThis() ? expr.getObjectExpression() : rewrite(expr.getObjectExpression()),rewrite(expr.getMethod()),rewrite(expr.getArguments()));
  conversion.setSafe(expr.isSafe());
  conversion.setSpreadSafe(expr.isSpreadSafe());
  conversion.setSourcePosition(expr);
  result=record(conversion,expr.getMethod());
}
