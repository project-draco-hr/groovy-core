{
  cn=cn.redirect();
  if (isPrimitiveType(cn))   return cn;
  if (cn == Boolean_TYPE) {
    return boolean_TYPE;
  }
 else   if (cn == Byte_TYPE) {
    return byte_TYPE;
  }
 else   if (cn == Character_TYPE) {
    return char_TYPE;
  }
 else   if (cn == Short_TYPE) {
    return short_TYPE;
  }
 else   if (cn == Integer_TYPE) {
    return int_TYPE;
  }
 else   if (cn == Long_TYPE) {
    return long_TYPE;
  }
 else   if (cn == Float_TYPE) {
    return float_TYPE;
  }
 else   if (cn == Double_TYPE) {
    return double_TYPE;
  }
 else {
    return cn;
  }
}
