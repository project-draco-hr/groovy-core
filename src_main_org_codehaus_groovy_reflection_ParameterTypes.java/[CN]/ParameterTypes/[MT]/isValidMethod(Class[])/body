{
  if (arguments == null)   return true;
  final int size=arguments.length;
  CachedClass[] pt=getParameterTypes();
  final int paramMinus1=pt.length - 1;
  if (size >= paramMinus1 && isVargsMethod)   return isValidVarargsMethod(arguments,size,pt,paramMinus1);
 else   if (pt.length == size)   return isValidExactMethod(arguments,size,pt);
 else   if (pt.length == 1 && size == 0)   return true;
  return false;
}
