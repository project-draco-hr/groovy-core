{
  SwingBuilder.checkValueIsType(value,name,Number.class);
  Object num;
  if (value != null) {
    num=value;
  }
 else {
    num=properties.remove("width");
  }
  if (num instanceof Number) {
    return Box.createHorizontalStrut(((Number)num).intValue());
  }
 else {
    return Box.createHorizontalStrut(6);
  }
}
