{
  NoSuchClassException e=assertCompileFailed("class UnknownClass {\n" + "    main() {\n" + "        try {\n"+ "            println('Hello World!')\n"+ "        }\n"+ "        catch (UnknownException e) {\n"+ "            println('This will never happen')\n"+ "        }\n"+ "    }\n"+ "}\n");
  assertEquals("UnknownException",e.getType());
}
