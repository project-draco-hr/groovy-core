{
  Map map;
  if (left instanceof TreeMap)   map=new TreeMap(left);
 else   if (left instanceof LinkedHashMap)   map=new LinkedHashMap(left);
 else   if (left instanceof Properties) {
    map=new Properties();
    map.putAll(left);
  }
 else   if (left instanceof Hashtable)   map=new Hashtable(left);
 else   map=new HashMap(left);
  return map;
}
