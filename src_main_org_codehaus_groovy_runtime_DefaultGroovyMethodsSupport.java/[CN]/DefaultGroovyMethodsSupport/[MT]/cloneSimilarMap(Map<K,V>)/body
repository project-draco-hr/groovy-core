{
  Map<K,V> answer=(Map<K,V>)cloneObject(orig);
  if (answer != null)   return answer;
  if (orig instanceof TreeMap)   return new TreeMap<K,V>(orig);
  if (orig instanceof Properties) {
    Map<K,V> map=(Map<K,V>)new Properties();
    map.putAll(orig);
    return map;
  }
  if (orig instanceof Hashtable)   return new Hashtable<K,V>(orig);
  return new LinkedHashMap<K,V>(orig);
}
