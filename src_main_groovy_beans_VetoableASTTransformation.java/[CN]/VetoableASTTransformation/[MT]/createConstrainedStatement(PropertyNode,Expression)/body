{
  Expression methodArguments=new ArgumentListExpression(new Expression[]{new ConstantExpression(propertyNode.getName()),fieldExpression,new VariableExpression("value")});
  if (vcsField == null) {
    return new ExpressionStatement(new MethodCallExpression(VariableExpression.THIS_EXPRESSION,"fireVetoableChange",methodArguments));
  }
 else {
    return new ExpressionStatement(new MethodCallExpression(new FieldExpression(vcsField),"fireVetoableChange",methodArguments));
  }
}
