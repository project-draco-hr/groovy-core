{
  onLineNumber(loop);
  Label l0=new Label();
  cv.visitLabel(l0);
  Label l1=new Label();
  Label previousContinueLable=continueLabel;
  continueLabel=l1;
  loop.getLoopBlock().visit(this);
  cv.visitLabel(l1);
  loop.getBooleanExpression().visit(this);
  cv.visitJumpInsn(IFNE,l0);
  continueLabel=previousContinueLable;
}
