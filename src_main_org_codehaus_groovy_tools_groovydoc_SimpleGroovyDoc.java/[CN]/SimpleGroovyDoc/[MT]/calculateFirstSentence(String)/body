{
  String text=raw.replaceAll("(?m)^\\s*\\*","");
  BreakIterator boundary=BreakIterator.getSentenceInstance(Locale.getDefault());
  boundary.setText(text);
  int start=boundary.first();
  int end=boundary.next();
  if (start > -1 && end > -1) {
    text=text.substring(start,end);
  }
  return text;
}
