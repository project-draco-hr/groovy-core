{
switch (target) {
case TYPE_TARGET:
    return "TYPE";
case CONSTRUCTOR_TARGET:
  return "CONSTRUCTOR";
case METHOD_TARGET:
return "METHOD";
case FIELD_TARGET:
return "FIELD";
case PARAMETER_TARGET:
return "PARAMETER";
case LOCAL_VARIABLE_TARGET:
return "LOCAL_VARIABLE";
case ANNOTATION_TARGET:
return "ANNOTATION";
default :
return "unknown target";
}
}
