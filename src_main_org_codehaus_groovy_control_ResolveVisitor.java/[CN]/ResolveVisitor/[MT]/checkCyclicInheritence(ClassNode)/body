{
  ClassNode sn=node;
  while (true) {
    sn=sn.getUnresolvedSuperClass();
    if (sn == null)     break;
    if (node == sn.redirect()) {
      addError("Cyclic inheritance involving " + sn.getName() + " in class "+ node.getName(),node);
      break;
    }
    if (sn == ClassHelper.OBJECT_TYPE)     break;
  }
}
