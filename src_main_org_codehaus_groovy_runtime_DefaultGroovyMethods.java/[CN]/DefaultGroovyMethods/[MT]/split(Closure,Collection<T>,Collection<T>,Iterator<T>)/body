{
  List<Collection<T>> answer=new ArrayList<Collection<T>>();
  while (iter.hasNext()) {
    T value=iter.next();
    if (DefaultTypeTransformation.castToBoolean(closure.call(value))) {
      accept.add(value);
    }
 else {
      reject.add(value);
    }
  }
  answer.add(accept);
  answer.add(reject);
  return answer;
}
