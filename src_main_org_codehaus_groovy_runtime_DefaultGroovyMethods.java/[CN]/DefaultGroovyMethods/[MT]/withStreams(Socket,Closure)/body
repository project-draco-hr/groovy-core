{
  InputStream input=socket.getInputStream();
  OutputStream output=socket.getOutputStream();
  try {
    closure.call(new Object[]{input,output});
  }
  finally {
    try {
      input.close();
    }
 catch (    IOException e) {
    }
    try {
      output.close();
    }
 catch (    IOException e) {
    }
  }
}
