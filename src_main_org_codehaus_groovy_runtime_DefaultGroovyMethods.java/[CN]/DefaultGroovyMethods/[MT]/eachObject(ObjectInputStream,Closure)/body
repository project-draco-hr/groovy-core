{
  try {
    while (true) {
      try {
        Object obj=ois.readObject();
        closure.call(obj);
      }
 catch (      EOFException e) {
        break;
      }
    }
    InputStream temp=ois;
    ois=null;
    temp.close();
  }
  finally {
    if (ois != null) {
      try {
        ois.close();
      }
 catch (      Exception e) {
        log.warning("Caught exception closing ObjectInputStream: " + e);
      }
    }
  }
}
