{
  StringBuilder buffer=new StringBuilder();
  boolean first=true;
  if (separator == null)   separator="";
  for (int i=0; i < self.length; i++) {
    String value=InvokerHelper.toString(self[i]);
    if (first) {
      first=false;
    }
 else {
      buffer.append(separator);
    }
    buffer.append(value);
  }
  return buffer.toString();
}
