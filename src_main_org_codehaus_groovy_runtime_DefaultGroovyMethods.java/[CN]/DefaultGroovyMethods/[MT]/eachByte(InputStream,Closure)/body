{
  try {
    while (true) {
      int b=is.read();
      if (b == -1) {
        break;
      }
 else {
        closure.callSpecial(new Byte((byte)b));
      }
    }
    is.close();
  }
 catch (  IOException e) {
    if (is != null) {
      try {
        is.close();
      }
 catch (      Exception e2) {
      }
      throw e;
    }
  }
}
