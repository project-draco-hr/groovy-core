{
  try {
    while (true) {
      int b=is.read();
      if (b == -1) {
        break;
      }
 else {
        closure.call(new Byte((byte)b));
      }
    }
    InputStream temp=is;
    is=null;
    temp.close();
  }
  finally {
    if (is != null) {
      try {
        is.close();
      }
 catch (      IOException e) {
        LOG.warning("Caught exception closing InputStream: " + e);
      }
    }
  }
}
