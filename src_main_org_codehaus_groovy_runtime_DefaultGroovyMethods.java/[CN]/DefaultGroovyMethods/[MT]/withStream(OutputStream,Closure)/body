{
  try {
    closure.call(os);
    os.flush();
    OutputStream temp=os;
    os=null;
    temp.close();
  }
  finally {
    if (os != null) {
      try {
        os.close();
      }
 catch (      IOException e) {
        log.warning("Caught exception closing OutputStream: " + e);
      }
    }
  }
}
