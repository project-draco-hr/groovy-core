{
  final Set ansSet;
  if (self instanceof SortedSet) {
    ansSet=new TreeSet();
  }
 else {
    ansSet=new HashSet();
  }
  Comparator numberComparator=new NumberAwareComparator();
  for (Iterator it=self.iterator(); it.hasNext(); ) {
    Object o=it.next();
    if (numberComparator.compare(o,operand) != 0)     ansSet.add(o);
  }
  return ansSet;
}
