{
  Thread thread=new Thread(new Runnable(){
    public void run(){
      try {
        self.waitFor();
      }
 catch (      InterruptedException e) {
      }
    }
  }
);
  thread.start();
  try {
    long increment=numberOfMillis.longValue() / 10;
    long ellapsed=0;
    while (thread.isAlive() && ellapsed < numberOfMillis.longValue()) {
      Thread.sleep(increment);
      ellapsed+=increment;
    }
  }
 catch (  InterruptedException e) {
  }
  if (thread.isAlive()) {
    self.destroy();
    thread.interrupt();
  }
}
