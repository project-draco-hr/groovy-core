{
  if (self.size() <= num) {
    return createSimilarList(self,0);
  }
  if (num <= 0) {
    List<T> ret=createSimilarList(self,self.size());
    ret.addAll(self);
    return ret;
  }
  List<T> ret=createSimilarList(self,self.size() - num);
  ret.addAll(self.subList(num,self.size()));
  return ret;
}
