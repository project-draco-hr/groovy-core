{
  RangeInfo info=subListBorders(self.size(),range);
  List sublist=self.subList(info.from,info.to);
  sublist.clear();
  if (value instanceof Collection) {
    Collection col=(Collection)value;
    if (col.size() == 0)     return;
    sublist.addAll(col);
  }
 else {
    sublist.add(value);
  }
}
