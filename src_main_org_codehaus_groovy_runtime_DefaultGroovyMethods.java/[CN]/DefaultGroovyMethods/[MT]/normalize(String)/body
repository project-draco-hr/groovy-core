{
  BufferedReader br=new BufferedReader(new StringReader(self));
  StringBuilder sb=new StringBuilder(self.length());
  boolean first=true;
  try {
    while (true) {
      String line=br.readLine();
      if (line == null) {
        break;
      }
 else {
        if (first)         first=false;
 else         sb.append("\n");
        sb.append(line);
      }
    }
  }
  finally {
    closeWithWarning(br);
  }
  return sb.toString();
}
