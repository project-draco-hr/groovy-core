{
  if (clazz == List.class) {
    return asList(col);
  }
 else   if (clazz == Set.class) {
    if (col instanceof Set)     return col;
    return new HashSet(col);
  }
 else   if (clazz == SortedSet.class) {
    if (col instanceof TreeSet)     return col;
    return new TreeSet(col);
  }
  return asType((Object)col,clazz);
}
