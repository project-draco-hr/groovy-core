{
  int result=-1;
  int i=0;
  for (Iterator iter=InvokerHelper.asIterator(self); iter.hasNext(); i++) {
    Object value=iter.next();
    if (i < startIndex) {
      continue;
    }
    if (DefaultTypeTransformation.castToBoolean(closure.call(value))) {
      result=i;
    }
  }
  return result;
}
