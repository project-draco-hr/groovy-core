{
  Object result=initialValue;
  Object[] param=new Object[1];
  for (Iterator iter=self.iterator(); iter.hasNext(); ) {
    param[0]=iter.next();
    if (first) {
      result=param[0];
      first=false;
      continue;
    }
    MetaClass metaClass=InvokerHelper.getMetaClass(result);
    result=metaClass.invokeMethod(result,"plus",param);
  }
  return result;
}
