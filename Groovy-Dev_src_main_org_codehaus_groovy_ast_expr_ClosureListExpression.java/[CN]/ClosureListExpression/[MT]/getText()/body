{
  StringBuffer buffer=new StringBuffer("(");
  boolean first=true;
  for (Iterator iter=getExpressions().iterator(); iter.hasNext(); ) {
    if (first) {
      first=false;
    }
 else {
      buffer.append("; ");
    }
    buffer.append(((Expression)iter.next()).getText());
  }
  buffer.append(")");
  return buffer.toString();
}
