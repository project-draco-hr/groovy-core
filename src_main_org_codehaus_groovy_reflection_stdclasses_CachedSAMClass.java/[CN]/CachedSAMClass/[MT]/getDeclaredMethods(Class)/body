{
  try {
    Method[] methods=AccessController.doPrivileged(new PrivilegedAction<Method[]>(){
      public Method[] run(){
        return c.getDeclaredMethods();
      }
    }
);
    if (methods != null)     return methods;
  }
 catch (  java.security.AccessControlException ace) {
  }
  return new Method[0];
}
