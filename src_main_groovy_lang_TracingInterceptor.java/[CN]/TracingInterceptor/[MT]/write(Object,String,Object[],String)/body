{
  try {
    writer.write("Interceptor ");
    writer.write(origin);
    writer.write(" ");
    Class theClass=object instanceof Class ? (Class)object : object.getClass();
    writeInfo(theClass,methodName,arguments);
    writer.write("\n");
    writer.flush();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
