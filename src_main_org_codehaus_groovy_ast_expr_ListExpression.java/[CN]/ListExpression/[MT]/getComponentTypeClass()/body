{
  boolean first=true;
  Class cls=null;
  for (Iterator iter=expressions.iterator(); iter.hasNext(); ) {
    Expression exp=(Expression)iter.next();
    if (cls == null) {
      cls=exp.getType().getTypeClass();
    }
 else {
      if (cls != exp.getType().getTypeClass()) {
        return null;
      }
    }
  }
  return cls;
}
