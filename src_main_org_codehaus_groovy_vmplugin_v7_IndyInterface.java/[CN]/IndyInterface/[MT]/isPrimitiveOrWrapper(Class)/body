{
  return c == byte.class || c == Byte.class || c == int.class || c == Integer.class || c == long.class || c == Long.class || c == float.class || c == Float.class || c == double.class || c == Double.class || c == short.class || c == Short.class || c == boolean.class || c == Boolean.class || c == char.class || c == Character.class;
}
