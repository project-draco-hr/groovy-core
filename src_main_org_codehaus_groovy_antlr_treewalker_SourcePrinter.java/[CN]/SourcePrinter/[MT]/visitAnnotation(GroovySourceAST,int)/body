{
  if (visit == OPENING_VISIT) {
    print(t,visit,"@");
  }
  if (visit == SUBSEQUENT_VISIT) {
    if (t.getNumberOfChildren() > 1) {
      print(t,visit,"(");
    }
  }
  if (visit == CLOSING_VISIT) {
    if (t.getNumberOfChildren() > 1) {
      print(t,visit,") ");
    }
 else {
      print(t,visit," ");
    }
  }
}
