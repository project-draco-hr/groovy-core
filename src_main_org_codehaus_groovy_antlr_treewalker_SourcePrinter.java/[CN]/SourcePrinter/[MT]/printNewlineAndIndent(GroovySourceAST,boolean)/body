{
  int currentLine=t.getLine();
  if (lastLinePrinted == 0) {
    lastLinePrinted=currentLine;
  }
  if (lastLinePrinted != currentLine || suggestNewline) {
    if (newLines) {
      out.println();
      for (int i=0; i < tabLevel; i++) {
        out.print("    ");
      }
    }
    lastLinePrinted=currentLine;
  }
}
