{
  out.printIndent();
  out.print("<");
  out.print(name.toString());
  for (Iterator iter=attributes.entrySet().iterator(); iter.hasNext(); ) {
    Map.Entry entry=(Map.Entry)iter.next();
    out.print(" ");
    print(entry.getKey());
    out.print("='");
    print(entry.getValue());
    out.print("'");
  }
  out.println(">");
  out.incrementIndent();
  newline=true;
  return name;
}
