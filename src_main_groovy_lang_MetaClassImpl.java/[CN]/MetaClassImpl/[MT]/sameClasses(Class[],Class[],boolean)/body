{
  if (params == null)   return false;
  if (params.length != arguments.length)   return false;
  for (int i=params.length - 1; i >= 0; i--) {
    Object arg=arguments[i];
    if (arg != null) {
      if (params[i] != arguments[i])       return false;
    }
 else     if (!weakNullCheck)     return false;
  }
  return true;
}
