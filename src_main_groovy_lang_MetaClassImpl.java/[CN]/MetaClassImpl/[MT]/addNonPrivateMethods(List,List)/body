{
  for (Iterator iter=oldList.iterator(); iter.hasNext(); ) {
    MetaMethod element=(MetaMethod)iter.next();
    if (element.isPrivate()) {
      MetaMethod match=removeMatchingMethod(newList,element);
      if (match != null) {
        newList.add(match);
      }
      continue;
    }
    addMethodToList(newList,element);
  }
}
