{
  if (innerMap == null) {
    for (int i=0; i < size; i++) {
      if (listKeys[i].equals(key)) {
        V old=(V)listValues[i];
        size--;
        listValues[i]=listValues[size];
        listKeys[i]=listKeys[size];
        return old;
      }
    }
    return null;
  }
 else {
    V old=innerMap.remove(key);
    size=innerMap.size();
    if (size <= maxListFill) {
      mapToList();
    }
    return old;
  }
}
