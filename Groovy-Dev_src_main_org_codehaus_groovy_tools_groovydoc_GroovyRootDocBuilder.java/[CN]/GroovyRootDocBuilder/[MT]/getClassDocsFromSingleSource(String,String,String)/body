{
  Map classDocsFromSrc=null;
  if (file.indexOf(".java") > 0) {
    classDocsFromSrc=parseJava(packagePath,file,src);
  }
 else   if (file.indexOf(".sourcefile") > 0) {
    classDocsFromSrc=parseJava(packagePath,file,src);
  }
 else {
    classDocsFromSrc=parseGroovy(packagePath,file,src);
  }
  return classDocsFromSrc;
}
