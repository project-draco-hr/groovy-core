{
  if (sig[numberOfArguments] == null) {
    StringBuilder sb=new StringBuilder("(");
    for (int i=0; i != numberOfArguments; ++i) {
      sb.append("Ljava/lang/Object;");
    }
    sb.append(")[Ljava/lang/Object;");
    sig[numberOfArguments]=sb.toString();
  }
  return sig[numberOfArguments];
}
