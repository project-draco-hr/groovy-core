{
  Connection connection=createConnection();
  Statement statement=connection.createStatement();
  try {
    log.debug(sql);
    return statement.execute(sql);
  }
 catch (  SQLException e) {
    log.warn("Failed to execute: " + sql,e);
    throw e;
  }
 finally {
    try {
      statement.close();
    }
 catch (    SQLException e) {
    }
    try {
      connection.close();
    }
 catch (    SQLException e) {
    }
  }
}
