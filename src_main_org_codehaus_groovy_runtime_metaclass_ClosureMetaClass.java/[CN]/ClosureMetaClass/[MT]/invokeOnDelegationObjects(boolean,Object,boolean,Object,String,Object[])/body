{
  MissingMethodException first=null;
  if (invoke1) {
    GroovyObject go=(GroovyObject)o1;
    try {
      return go.invokeMethod(methodName,args);
    }
 catch (    MissingMethodException mme) {
      first=mme;
    }
  }
  if (invoke2) {
    GroovyObject go=(GroovyObject)o2;
    try {
      return go.invokeMethod(methodName,args);
    }
 catch (    MissingMethodException mme) {
      if (first == null)       first=mme;
    }
  }
  throw first;
}
