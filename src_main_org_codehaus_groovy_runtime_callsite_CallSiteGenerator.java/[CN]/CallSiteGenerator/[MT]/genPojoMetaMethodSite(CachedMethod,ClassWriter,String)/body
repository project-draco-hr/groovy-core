{
  MethodVisitor mv;
  cw.visit(Opcodes.V1_4,Opcodes.ACC_PUBLIC,name.replace('.','/'),null,"org/codehaus/groovy/runtime/callsite/PojoMetaMethodSite",null);
  genConstructor(cw,"org/codehaus/groovy/runtime/callsite/PojoMetaMethodSite");
