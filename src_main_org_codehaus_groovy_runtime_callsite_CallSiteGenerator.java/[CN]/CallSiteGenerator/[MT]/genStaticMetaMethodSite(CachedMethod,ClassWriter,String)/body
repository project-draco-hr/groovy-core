{
  cw.visit(Opcodes.V1_4,Opcodes.ACC_PUBLIC | Opcodes.ACC_SYNTHETIC,name.replace('.','/'),null,"org/codehaus/groovy/runtime/callsite/StaticMetaMethodSite",null);
