{
  if (file == null) {
    try {
      if (isFile) {
        file=new BufferedReader(new FileReader(path));
      }
 else {
        if (sources.containsKey(path)) {
          file=new BufferedReader(new StringReader((String)sources.get(path)));
        }
      }
    }
 catch (    Exception e) {
      file=null;
    }
  }
  return file != null;
}
