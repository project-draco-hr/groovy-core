{
  if (open() && lineNumber < number) {
    reopen();
  }
  if (file != null) {
    boolean done=(lineNumber < 0);
    while (number < lineNumber && !done) {
      try {
        line=file.readLine();
        number++;
      }
 catch (      Exception e) {
        close();
        done=true;
      }
    }
  }
  return line;
}
