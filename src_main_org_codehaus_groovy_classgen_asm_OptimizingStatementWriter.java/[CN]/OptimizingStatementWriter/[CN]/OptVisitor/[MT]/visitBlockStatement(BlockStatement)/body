{
  boolean optAll=true;
  for (  Statement statement : block.getStatements()) {
    opt.reset();
    statement.visit(this);
    optAll=optAll && opt.canOptimize();
  }
  if (block.isEmpty()) {
    opt.canOptimize=true;
  }
 else {
    opt.shouldOptimize=optAll;
    if (optAll)     addMeta(block);
  }
}
