{
  List sorted=new ArrayList(unsorted.size());
  int start=0;
  for (int i=0; i < unsorted.size(); i++) {
    int min=-1;
    for (int j=0; j < unsorted.size(); j++) {
      if (index[j] == -1)       continue;
      if (min == -1) {
        min=j;
      }
 else       if (index[j] < index[min]) {
        min=j;
      }
    }
    if (min == -1)     break;
    sorted.add(unsorted.get(min));
    index[min]=-1;
  }
  return sorted;
}
