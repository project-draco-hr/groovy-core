{
  if (type == ClassHelper.double_TYPE) {
    mv.visitInsn(D2I);
    mv.visitInsn(I2B);
  }
 else   if (type == ClassHelper.long_TYPE) {
    mv.visitInsn(L2I);
    mv.visitInsn(I2B);
  }
 else   if (type == ClassHelper.float_TYPE) {
    mv.visitInsn(F2I);
    mv.visitInsn(I2B);
  }
 else   if (type == ClassHelper.int_TYPE) {
    mv.visitInsn(I2B);
  }
}
