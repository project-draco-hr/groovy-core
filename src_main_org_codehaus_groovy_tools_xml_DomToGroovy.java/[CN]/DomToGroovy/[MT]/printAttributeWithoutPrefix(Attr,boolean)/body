{
  String prefix=attribute.getPrefix();
  if (prefix == null || prefix.length() == 0) {
    if (!hasAttribute) {
      hasAttribute=true;
    }
 else {
      print(", ");
    }
    print(getLocalName(attribute));
    print(":'");
    print(getAttributeValue(attribute));
    print("'");
  }
  return hasAttribute;
}
