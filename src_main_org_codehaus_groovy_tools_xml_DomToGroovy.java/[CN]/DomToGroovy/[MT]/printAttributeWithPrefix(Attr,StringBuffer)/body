{
  String prefix=attribute.getPrefix();
  if (prefix != null && prefix.length() > 0) {
    if (buffer.length() > 0) {
      buffer.append(", ");
    }
    buffer.append(prefix);
    buffer.append(".");
    buffer.append(getLocalName(attribute));
    buffer.append(":'");
    buffer.append(attribute.getValue());
    buffer.append("'");
  }
}
