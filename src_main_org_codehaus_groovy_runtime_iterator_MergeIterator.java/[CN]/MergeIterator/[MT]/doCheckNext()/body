{
  if (isSecond) {
    if (!second.hasNext()) {
      hasNext=first.hasNext();
      if (hasNext)       lastAcquired=first.next();
    }
 else {
      hasNext=true;
      lastAcquired=second.next();
    }
    isSecond=false;
  }
 else {
    if (!first.hasNext()) {
      hasNext=second.hasNext();
      if (hasNext)       lastAcquired=second.next();
    }
 else {
      hasNext=true;
      lastAcquired=first.next();
    }
    isSecond=true;
  }
}
