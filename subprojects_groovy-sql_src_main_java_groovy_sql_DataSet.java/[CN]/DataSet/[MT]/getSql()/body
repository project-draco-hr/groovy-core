{
  if (sql == null) {
    sql="select * from " + table;
    String whereClaus=getSqlWhere();
    if (whereClaus.length() > 0) {
      sql+=" where " + whereClaus;
    }
    String orerByClaus=getSqlOrderBy();
    if (orerByClaus.length() > 0) {
      sql+=" order by " + orerByClaus;
    }
  }
  return sql;
}
