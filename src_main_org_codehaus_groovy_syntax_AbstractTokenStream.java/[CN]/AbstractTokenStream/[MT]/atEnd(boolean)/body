{
  try {
    int offset=1;
    if (ignoringWhitespace) {
      try {
        while (la(offset).isA(Token.NEWLINE)) {
          offset++;
        }
      }
 catch (      Exception e) {
      }
    }
    if (la(offset) == null) {
      return true;
    }
  }
 catch (  Exception e) {
  }
  return false;
}
