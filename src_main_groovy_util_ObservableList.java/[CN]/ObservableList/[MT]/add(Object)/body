{
  int oldSize=size();
  boolean success=delegate.add(o);
  if (success) {
    if (test != null) {
      Object result=test.call(o);
      if (result != null && result instanceof Boolean && ((Boolean)result).booleanValue()) {
        fireElementAddedEvent(size() - 1,o);
        fireSizeChangedEvent(oldSize,size());
      }
    }
 else {
      fireElementAddedEvent(size() - 1,o);
      fireSizeChangedEvent(oldSize,size());
    }
  }
  return success;
}
