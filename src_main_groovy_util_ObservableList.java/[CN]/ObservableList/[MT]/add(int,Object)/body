{
  delegate.add(index,element);
  if (test != null) {
    Object result=test.call(element);
    if (result != null && result instanceof Boolean && ((Boolean)result).booleanValue()) {
      pcs.firePropertyChange(new ElementAddedEvent(this,element,index));
    }
  }
 else {
    pcs.firePropertyChange(new ElementAddedEvent(this,element,index));
  }
}
