{
  try {
    statement.close();
  }
 catch (  SQLException e) {
    log.log(Level.SEVERE,"Caught exception closing statement: " + e,e);
  }
  if (dataSource != null) {
    try {
      connection.close();
    }
 catch (    SQLException e) {
      log.log(Level.SEVERE,"Caught exception closing connection: " + e,e);
    }
  }
}
