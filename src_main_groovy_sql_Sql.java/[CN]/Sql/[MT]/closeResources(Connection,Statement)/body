{
  if (isCacheStatements())   return;
  if (statement != null) {
    try {
      statement.close();
    }
 catch (    SQLException e) {
      log.log(Level.INFO,"Caught exception closing statement: " + e,e);
    }
  }
  if (dataSource != null) {
    try {
      connection.close();
    }
 catch (    SQLException e) {
      log.log(Level.INFO,"Caught exception closing connection: " + e,e);
    }
  }
}
