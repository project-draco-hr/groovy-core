{
  if (__index + TRUE.length <= charArray.length) {
    if (charArray[__index] == 't' && charArray[++__index] == 'r' && charArray[++__index] == 'u' && charArray[++__index] == 'e') {
      __index++;
      return true;
    }
  }
  throw new JsonException(exceptionDetails("true not parsed properly"));
}
