{
  paths=classpath.split(File.pathSeparator);
  for (int i=0; i < paths.length; i++) {
    File path=new File(base,paths[i]);
    if (path.exists()) {
      if (!path.isDirectory()) {
        try {
          JarFile jar=new JarFile(path);
          pathList.add(paths[i]);
        }
 catch (        IOException e) {
          continue;
        }
      }
 else {
        pathList.add(paths[i]);
      }
    }
  }
}
