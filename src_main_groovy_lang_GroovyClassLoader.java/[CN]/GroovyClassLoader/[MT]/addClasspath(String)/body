{
  AccessController.doPrivileged(new PrivilegedAction<Void>(){
    public Void run(){
      try {
        File f=new File(path);
        URL newURL=f.toURI().toURL();
        URL[] urls=getURLs();
        for (        URL url : urls) {
          if (url.equals(newURL))           return null;
        }
        addURL(newURL);
      }
 catch (      MalformedURLException e) {
      }
      return null;
    }
  }
);
}
