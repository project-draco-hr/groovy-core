{
  if (file == null) {
    throw new NullPointerException("file");
  }
  if (template == null) {
    throw new NullPointerException("template");
  }
  if (timestamp) {
    this.date=new Date(System.currentTimeMillis());
  }
 else {
    this.date=null;
  }
  this.hit=0;
  this.lastModified=file.lastModified();
  this.length=file.length();
  this.template=template;
}
