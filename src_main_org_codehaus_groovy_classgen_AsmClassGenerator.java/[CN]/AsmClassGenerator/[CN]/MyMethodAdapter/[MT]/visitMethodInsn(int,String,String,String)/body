{
  if (boxing(opcode,owner,name)) {
    boxingDesc=desc;
    dropBoxing();
  }
 else {
    if (unboxing(opcode,owner,name)) {
      if (boxingDesc != null)       boxingDesc=null;
 else       super.visitMethodInsn(opcode,owner,name,desc);
    }
 else {
      dropBoxing();
      super.visitMethodInsn(opcode,owner,name,desc);
    }
  }
}
