{
  Expression objectExpression=call.getObjectExpression();
  if (!isStaticMethod() && isThisExpression(call.getObjectExpression())) {
    objectExpression=new CastExpression(ClassHelper.make(GroovyObject.class),objectExpression);
  }
  Expression messageName=new CastExpression(ClassHelper.STRING_TYPE,call.getMethod());
  if (useSuper) {
    makeCall(new ClassExpression(classNode.getSuperClass()),objectExpression,messageName,call.getArguments(),adapter,call.isSafe(),call.isSpreadSafe());
  }
 else {
    makeCall(objectExpression,messageName,call.getArguments(),adapter,call.isSafe(),call.isSpreadSafe());
  }
}
