{
  String text=expression.getText();
  try {
    return resolveClassName(text);
  }
 catch (  Exception e) {
    if (text.endsWith(".class")) {
      text=text.substring(0,text.length() - 6);
      try {
        return resolveClassName(text);
      }
 catch (      Exception e2) {
      }
    }
    return null;
  }
}
