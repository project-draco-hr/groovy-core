{
  Statement stmt;
  if (cacheStatements) {
synchronized (statementCache) {
      stmt=statementCache.get(sql);
      if (stmt == null) {
        stmt=cmd.execute(connection,sql);
        statementCache.put(sql,stmt);
      }
    }
  }
 else {
    stmt=cmd.execute(connection,sql);
  }
  return stmt;
}
