{
synchronized (script) {
    MetaClass scriptMetaClass=script.getMetaClass();
    try {
      script.setMetaClass(new FactoryInterceptorMetaClass(scriptMetaClass,this));
      script.setBinding(this);
      return script.run();
    }
  finally {
      script.setMetaClass(scriptMetaClass);
    }
  }
}
