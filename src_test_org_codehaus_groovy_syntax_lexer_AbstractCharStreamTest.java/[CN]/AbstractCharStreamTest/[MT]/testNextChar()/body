{
  MockCharStream in=new MockCharStream("cheddar");
  assertEquals('c',in.nextChar());
  assertEquals('h',in.nextChar());
  assertEquals('e',in.nextChar());
  assertEquals('d',in.nextChar());
  assertEquals('d',in.nextChar());
  assertEquals('a',in.nextChar());
  assertEquals('r',in.nextChar());
  assertEquals((char)-1,in.nextChar());
}
