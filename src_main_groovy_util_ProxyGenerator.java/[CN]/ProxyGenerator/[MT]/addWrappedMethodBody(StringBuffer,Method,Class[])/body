{
  buffer.append("\n        Object[] args = [");
  for (int j=0; j < parameterTypes.length; j++) {
    if (j != 0) {
      buffer.append(", ");
    }
    buffer.append("p").append(j);
  }
  buffer.append("]\n        ");
  buffer.append("InvokerHelper.invokeMethod(delegate, '").append(method.getName()).append("', args)\n");
}
