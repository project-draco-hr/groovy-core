{
  for (int i=0; i < publicAndProtectedMethods.size(); i++) {
    Method method=(Method)publicAndProtectedMethods.get(i);
    if (candidate.getName().equals(method.getName()) && candidate.getParameterTypes().length == method.getParameterTypes().length && candidate.getReturnType().equals(method.getReturnType())) {
      return true;
    }
  }
  return false;
}
