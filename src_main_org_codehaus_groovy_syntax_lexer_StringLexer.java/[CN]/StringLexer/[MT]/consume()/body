{
  if (!finished && source != null) {
    char c=CharStream.EOS;
    if (delimited) {
      if (lookahead < 1) {
        la(1);
      }
      if (lookahead >= 1) {
        c=characters[0];
        for (int i=0; i < widths[0]; i++) {
          source.consume();
        }
        lookahead=0;
      }
      if (la(1) == CharStream.EOS) {
        finishUp();
      }
    }
 else {
      c=source.consume();
    }
    lookahead=0;
    return c;
  }
  return CharStream.EOS;
}
