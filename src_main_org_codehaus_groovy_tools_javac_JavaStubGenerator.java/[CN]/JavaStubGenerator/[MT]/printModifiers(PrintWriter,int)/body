{
  if ((modifiers & Opcodes.ACC_PUBLIC) != 0)   out.print("public ");
  if ((modifiers & Opcodes.ACC_PROTECTED) != 0)   out.print("protected ");
  if ((modifiers & Opcodes.ACC_PRIVATE) != 0)   out.print("private ");
  if ((modifiers & Opcodes.ACC_STATIC) != 0)   out.print("static ");
  if ((modifiers & Opcodes.ACC_SYNCHRONIZED) != 0)   out.print("synchronized ");
  if ((modifiers & Opcodes.ACC_ABSTRACT) != 0)   out.print("abstract ");
}
