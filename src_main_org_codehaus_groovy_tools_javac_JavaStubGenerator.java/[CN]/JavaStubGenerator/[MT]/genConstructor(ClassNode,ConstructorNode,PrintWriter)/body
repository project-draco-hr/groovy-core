{
  out.print("public ");
  out.print(clazz.getNameWithoutPackage());
  printParams(constructorNode,out);
  ConstructorCallExpression constrCall=getConstructorCallExpression(constructorNode);
  if (constrCall == null || !constrCall.isSpecialCall()) {
    out.println(" {}");
  }
 else {
    out.println(" {");
    genSpecialConstructorArgs(out,constructorNode,constrCall);
    out.println("}");
  }
  Parameter[] params=getDefaultValueReducedParameters(constructorNode);
  if (params != constructorNode.getParameters()) {
    ConstructorNode m=new ConstructorNode(constructorNode.getModifiers(),params,constructorNode.getExceptions(),constructorNode.getCode());
    genConstructor(clazz,m,out);
  }
}
