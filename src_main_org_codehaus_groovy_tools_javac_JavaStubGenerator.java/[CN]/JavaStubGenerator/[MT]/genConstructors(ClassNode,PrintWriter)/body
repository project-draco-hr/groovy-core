{
  List<ConstructorNode> constrs=classNode.getDeclaredConstructors();
  if (constrs != null)   for (  ConstructorNode constr : constrs) {
    genConstructor(classNode,constr,out);
  }
  if (defaultConstructor) {
    out.print("public ");
    String className=classNode.getNameWithoutPackage();
    if (classNode instanceof InnerClassNode)     className=className.substring(className.lastIndexOf("$") + 1);
    out.print(className);
    out.println("(){}");
  }
}
