{
  if (retName.equals("int") || retName.equals("byte") || retName.equals("short")|| retName.equals("long")|| retName.equals("float")|| retName.equals("double")|| retName.equals("char")) {
    out.print("(");
    out.print(retName);
    out.print(")");
    out.print("0");
  }
 else   if (retName.equals("boolean")) {
    out.print("false");
  }
 else {
    out.print("null");
  }
}
