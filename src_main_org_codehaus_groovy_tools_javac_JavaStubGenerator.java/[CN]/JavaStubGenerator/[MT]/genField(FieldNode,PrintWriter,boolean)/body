{
  if ((fieldNode.getModifiers() & Opcodes.ACC_PRIVATE) != 0)   return;
  printAnnotations(out,fieldNode);
  if (!isInterface) {
    printModifiers(out,fieldNode.getModifiers());
  }
  printType(fieldNode.getType(),out);
  out.print(" ");
  out.print(fieldNode.getName());
  if (isInterface) {
    out.print(" = null");
  }
  out.println(";");
}
