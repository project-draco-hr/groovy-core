{
  if (isAnExpression(node,unknownReturns)) {
    if (isAVariable(node)) {
      return true;
    }
 else     if (node.getMeaning() == Types.SYNTH_LIST) {
      boolean is=true;
      for (int i=1; i < node.size(); i++) {
        if (!isAModifiableExpression(node.get(i),unknownReturns)) {
          is=false;
          break;
        }
      }
      return is;
    }
  }
  return false;
}
