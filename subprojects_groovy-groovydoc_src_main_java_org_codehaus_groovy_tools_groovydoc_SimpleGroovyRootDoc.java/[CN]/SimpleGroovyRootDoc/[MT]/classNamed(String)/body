{
  for (  String key : classDocs.keySet()) {
    if (key.equals(name))     return classDocs.get(key);
    int lastSlashIdx=key.lastIndexOf('/');
    if (lastSlashIdx > 0) {
      String shortKey=key.substring(lastSlashIdx + 1);
      if (shortKey.equals(name)) {
        return classDocs.get(key);
      }
    }
  }
  return null;
}
