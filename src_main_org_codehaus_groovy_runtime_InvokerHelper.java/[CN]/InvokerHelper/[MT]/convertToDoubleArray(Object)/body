{
  double[] ans=null;
  if (a.getClass().getName().equals("[D")) {
    ans=(double[])a;
  }
 else {
    Object[] ia=(Object[])a;
    ans=new double[ia.length];
    for (int i=0; i < ia.length; i++) {
      if (ia[i] == null) {
        continue;
      }
      ans[i]=((Number)ia[i]).doubleValue();
    }
  }
  return ans;
}
