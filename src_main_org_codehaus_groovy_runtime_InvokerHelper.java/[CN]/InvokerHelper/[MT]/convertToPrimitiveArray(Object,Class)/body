{
  if (type == Byte.TYPE)   return convertToByteArray(a);
  if (type == Boolean.TYPE)   return convertToBooleanArray(a);
  if (type == Short.TYPE)   return convertToShortArray(a);
  if (type == Character.TYPE)   return convertToCharArray(a);
  if (type == Integer.TYPE)   return convertToIntArray(a);
  if (type == Long.TYPE)   return convertToLongArray(a);
  if (type == Float.TYPE)   return convertToFloatArray(a);
  if (type == Double.TYPE)   return convertToDoubleArray(a);
 else   return a;
}
