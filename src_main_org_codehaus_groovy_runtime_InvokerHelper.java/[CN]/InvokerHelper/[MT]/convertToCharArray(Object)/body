{
  char[] ans=null;
  if (a.getClass().getName().equals("[C")) {
    ans=(char[])a;
  }
 else {
    Object[] ia=(Object[])a;
    ans=new char[ia.length];
    for (int i=0; i < ia.length; i++) {
      if (ia[i] == null)       continue;
      ans[i]=((Character)ia[i]).charValue();
    }
  }
  return ans;
}
