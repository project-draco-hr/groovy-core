{
  StringBuffer buffer=new StringBuffer("[");
  boolean first=true;
  for (Iterator iter=list.iterator(); iter.hasNext(); ) {
    if (first) {
      first=false;
    }
 else {
      buffer.append(", ");
    }
    buffer.append(format(iter.next(),verbose));
  }
  buffer.append("]");
  return buffer.toString();
}
