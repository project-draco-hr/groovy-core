{
  String getterName=getGetterName(pNode);
  Expression selfGetter=callThisX(getterName);
  Expression otherGetter=callX(other,getterName);
  return new BooleanExpression(or(and(identicalX(selfGetter,var("this")),differentX(otherGetter,other)),and(differentX(selfGetter,var("this")),identicalX(otherGetter,other))));
}
