{
  final Expression fieldExpr=new VariableExpression(fNode);
  final Expression otherExpr=new PropertyExpression(other,fNode.getName());
  return new BooleanExpression(or(and(identicalX(fieldExpr,var("this")),differentX(otherExpr,other)),and(differentX(fieldExpr,var("this")),identicalX(otherExpr,other))));
}
