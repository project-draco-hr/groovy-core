{
  OperandStack operandStack=controller.getOperandStack();
  int m1=operandStack.getStackLength();
  prepareSiteAndReceiver(receiver,message,false,controller.getCompileStack().isLHS());
  visitBoxedArgument(arguments);
  int m2=operandStack.getStackLength();
  MethodVisitor mv=controller.getMethodVisitor();
  mv.visitMethodInsn(INVOKESTATIC,"org/codehaus/groovy/runtime/DefaultGroovyMethods","plus","(Ljava/lang/String;Ljava/lang/Object;)Ljava/lang/String;");
  controller.getOperandStack().replace(STRING_TYPE,m2 - m1);
}
