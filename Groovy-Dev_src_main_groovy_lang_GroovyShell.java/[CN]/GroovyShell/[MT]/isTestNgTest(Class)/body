{
  char version=System.getProperty("java.version").charAt(2);
  if (version < '5') {
    return false;
  }
  boolean isTest=false;
  try {
    if (InvokerHelper.invokeStaticMethod("org.codehaus.groovy.vmplugin.v5.TestNgUtils","realIsTestNgTest",new Object[]{scriptClass,this.loader}) == Boolean.TRUE) {
      isTest=true;
    }
    ;
  }
 catch (  ClassNotFoundException e) {
    throw new GroovyRuntimeException("Failed to invoke the TestNG helper class.");
  }
  return isTest;
}
