{
  String charString;
  if (c == ' ') {
    charString="[SPACE]";
  }
 else   if (c == '\t') {
    charString="[TAB]";
  }
 else   if (c == '\n') {
    charString="[NEWLINE]";
  }
 else {
    charString="'" + (char)c + "'";
  }
  charString=charString + " with an int value of " + ((int)c);
  return charString;
}
