{
  Objects.requireNonNull(array);
  int nullCount=0;
  for (  char[] ch : array) {
    if (ch == null || ch.length == 0) {
      nullCount++;
    }
  }
  char[][] newArray=new char[array.length - nullCount][];
  int j=0;
  for (  char[] ch : array) {
    if (ch == null || ch.length == 0) {
      continue;
    }
    newArray[j]=ch;
    j++;
  }
  return newArray;
}
