{
  int sign=1;
  boolean negative=false;
  int startIndex=0;
  if (buffer[startIndex] == '-') {
    startIndex++;
    sign=-1;
    negative=true;
  }
  if (CharScanner.isInteger(buffer,startIndex,location - startIndex,negative)) {
    return intValue() * sign;
  }
 else {
    return longValue() * sign;
  }
}
