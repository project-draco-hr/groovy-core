{
  GroovyAnnotationRef[] annotations=elem.annotations();
  if (annotations != null) {
    for (    GroovyAnnotationRef annotation : annotations) {
      if ("Deprecated".equals(annotation.name()) || "java.lang.Deprecated".equals(annotation.name())) {
        elem.setDeprecated(true);
        break;
      }
    }
  }
}
