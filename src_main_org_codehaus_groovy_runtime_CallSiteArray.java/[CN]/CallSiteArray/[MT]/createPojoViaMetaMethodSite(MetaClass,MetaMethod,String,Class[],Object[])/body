{
  if (metaMethod.correctArguments(args) == args) {
    if (noWrappers(args)) {
      if (noCoerce(metaMethod,args))       return new PojoViaMetaMethodSiteNoUnwrap(name,(MetaClassImpl)metaClass,metaMethod,params);
 else       return new PojoViaMetaMethodSiteNoUnwrapNoCoerce(name,(MetaClassImpl)metaClass,metaMethod,params);
    }
  }
  return new PojoViaMetaMethodSite(name,(MetaClassImpl)metaClass,metaMethod,params);
}
