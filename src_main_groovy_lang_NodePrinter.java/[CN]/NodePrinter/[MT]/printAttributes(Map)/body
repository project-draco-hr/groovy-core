{
  print("(");
  boolean first=true;
  for (Iterator iter=attributes.entrySet().iterator(); iter.hasNext(); ) {
    Map.Entry entry=(Map.Entry)iter.next();
    if (first) {
      first=false;
    }
 else {
      print(", ");
    }
    print(entry.getKey().toString());
    print(":");
    print(InvokerHelper.toString(entry.getValue()));
  }
  print(")");
}
