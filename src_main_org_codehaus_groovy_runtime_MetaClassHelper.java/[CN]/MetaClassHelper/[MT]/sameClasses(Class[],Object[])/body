{
  if (params.length != arguments.length)   return false;
  for (int i=params.length - 1; i >= 0; i--) {
    Object arg=arguments[i];
    if (arg == null || (params[i] != arg.getClass() && (!(arg instanceof Wrapper) || params[i] != ((Wrapper)arg).getType())))     return false;
  }
  return true;
}
