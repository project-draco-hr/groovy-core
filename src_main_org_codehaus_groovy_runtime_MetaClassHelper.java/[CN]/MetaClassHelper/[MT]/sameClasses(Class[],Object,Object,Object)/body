{
  if (params.length != 3)   return false;
  if (arg1 == null || (params[0] != arg1.getClass() && (!(arg1 instanceof Wrapper) || params[0] != ((Wrapper)arg1).getType())))   return false;
  if (arg2 == null || (params[1] != arg2.getClass() && (!(arg2 instanceof Wrapper) || params[1] != ((Wrapper)arg2).getType())))   return false;
  if (arg3 == null || (params[2] != arg3.getClass() && (!(arg3 instanceof Wrapper) || params[2] != ((Wrapper)arg3).getType())))   return false;
  return true;
}
