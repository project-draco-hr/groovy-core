{
  if (args == null)   return null;
  int s=args.length;
  Class[] ans=new Class[s];
  for (int i=0; i < s; i++) {
    Object o=args[i];
    if (o == null) {
      ans[i]=null;
    }
 else     if (o instanceof Wrapper) {
      ans[i]=((Wrapper)o).getType();
    }
 else {
      ans[i]=o.getClass();
    }
  }
  return ans;
}
