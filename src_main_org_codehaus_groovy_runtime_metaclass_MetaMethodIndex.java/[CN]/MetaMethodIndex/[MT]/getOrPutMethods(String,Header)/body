{
  final Class cls=header.cls;
  int h=hash(header.clsHashCode31 + name.hashCode());
  final Entry[] t=table;
  final int index=h & (t.length - 1);
  Entry e=t[index];
  for (; e != null; e=e.nextHashEntry)   if (e.hash == h && cls == e.cls && e.name.equals(name))   return e;
  Entry entry=new Entry();
  entry.nextHashEntry=t[index];
  entry.hash=h;
  entry.name=name;
  entry.cls=cls;
  t[index]=entry;
  entry.nextClassEntry=header.head;
  header.head=entry;
  if (++size == threshold)   resize(2 * t.length);
  return entry;
}
