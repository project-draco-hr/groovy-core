{
  returnAST=null;
  ASTPair currentAST=new ASTPair();
  AST constructorStart_AST=null;
  Token id=null;
  AST id_AST=null;
  modifiersOpt();
  id=LT(1);
  id_AST=astFactory.create(id);
  match(IDENT);
  if (!(isConstructorIdent(id)))   throw new SemanticException("isConstructorIdent(id)");
  nls();
  match(LPAREN);
  returnAST=constructorStart_AST;
}
