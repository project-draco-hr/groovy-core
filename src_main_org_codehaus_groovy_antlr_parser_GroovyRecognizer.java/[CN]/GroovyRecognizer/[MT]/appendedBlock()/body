{
  returnAST=null;
  ASTPair currentAST=new ASTPair();
  AST appendedBlock_AST=null;
  expressionBlock();
  if (inputState.guessing == 0) {
    astFactory.addASTChild(currentAST,returnAST);
  }
  if (inputState.guessing == 0) {
    appendedBlock_AST=(AST)currentAST.root;
  }
  returnAST=appendedBlock_AST;
}
