{
  int hash=e.hash;
  int index=index(hash,table.length);
  Entry current=table[index];
  Entry prev=null;
  while (current != null && current != e) {
    prev=current;
    current=current.next;
  }
  if (current == null)   return;
  current=current.invalidate();
  if (prev == null) {
    table[index]=current;
  }
 else {
    prev.next=current;
  }
  size--;
}
